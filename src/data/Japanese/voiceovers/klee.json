{
	"id": 10000029,
	"name": "Klee",
	"friendLines": [
		{
			"voicelineId": 18009,
			"title": "初めまして…",
			"voicelineType": "1000",
			"description": "西風騎士団、「火花騎士」クレー、参上！…えっと、この後は何だっけ？クレー覚えられないよ…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_draw_appear"
		},
		{
			"voicelineId": 18010,
			"title": "世間話·収穫",
			"voicelineType": "1101",
			"description": "今日ね、クレー変わった模様のトカゲを捕まえたんだ！見たことない模様だから、見てみる？",
			"voicefile": "VO_friendship/VO_klee/vo_klee_explore_idle_01"
		},
		{
			"voicelineId": 18011,
			"title": "世間話·冒険",
			"voicelineType": "1102",
			"description": "クレーと一緒にお魚をドカーンしにいく？バレたら1日反省室に閉じ込められるけど、お魚は美味しいからやりがいあるよ！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_explore_idle_02"
		},
		{
			"voicelineId": 18012,
			"title": "世間話·心得",
			"voicelineType": "1103",
			"description": "「城内爆破は反省室」、「怪我人いるところにジン団長あり」、「山火事は一巻の終わり」——これはクレーの騎士団で生きるための心得だよ。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_explore_idle_03"
		},
		{
			"voicelineId": 18013,
			"title": "雨の日…",
			"voicelineType": "1201",
			"description": "ふふん、今回の爆弾は防水だからへっちゃら。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_weather_heavyRain_01"
		},
		{
			"voicelineId": 18014,
			"title": "雷の日…",
			"voicelineType": "1202",
			"description": "何かが爆発した…！？…あっ、雷か。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_weather_thunder_01"
		},
		{
			"voicelineId": 18015,
			"title": "暴風の日…",
			"voicelineType": "1203",
			"description": "風神様、クレーの爆弾を必要な場所につれてってー！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_weather_gale_01"
		},
		{
			"voicelineId": 18016,
			"title": "おはよう…",
			"voicelineType": "1001",
			"description": "おはよー！クレーをあそびに連れてって！冒険しようよ！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_greetingMorning"
		},
		{
			"voicelineId": 18017,
			"title": "こんにちは…",
			"voicelineType": "1002",
			"description": "お昼ごはんの時間だ！なに食べようかな？『テイワット観光ガイド』でも見てみよう…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_greetingNoon"
		},
		{
			"voicelineId": 18018,
			"title": "こんばんは…",
			"voicelineType": "1003",
			"description": "こんばんは！クレーはもう子供じゃないってホフマンさんに言ってよ、クレーは夜も遊びに出かけたいの！遊びにつれてってー！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_greetingNight"
		},
		{
			"voicelineId": 18019,
			"title": "おやすみ…",
			"voicelineType": "1004",
			"description": "うう…出かける時帰りたくないって言ったけど、暗くなると道が分からないよ…ね、ねえ、帰るとき、クレーを送ってくれないかな…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_greetingNight2"
		},
		{
			"voicelineId": 18020,
			"title": "クレー自身について·トラブル",
			"voicelineType": "2001",
			"description": "あうっ…爆弾で風車の羽をぐにゃぐにゃしちゃった…ごめんなさい！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_idle"
		},
		{
			"voicelineId": 18021,
			"title": "クレー自身について·賠償",
			"voicelineType": "2002",
			"description": "ク、クレーは手作業が得意なの。だから新しいのを作るから…え？児童の労働禁止？ううう…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_idle2"
		},
		{
			"voicelineId": 18022,
			"title": "騎士について…",
			"voicelineType": "2003",
			"description": "ふふん…栄誉騎士、クレーは「火花騎士」として西風騎士団の先輩なんだよ？だからね！だから…クレーが起こしたトラブル、内緒にしてくれないかな…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_close",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "シナリオ「涙のない明日のために」をクリアする",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						38406
					]
				}
			]
		},
		{
			"voicelineId": 18023,
			"title": "遊び仲間について…",
			"voicelineType": "2004",
			"description": "だめ、まだ9時だよ！今日は絶対サーカス団のミッドナイト公演が終わるまで帰らないもん！クレーの仲間なんでしょ、帰らせようだなんて考えないでよ…ペペぺ。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_close1_b",
			"voicefile_male": "VO_friendship/VO_klee/vo_klee_dialog_close1_a",
			"hasGenderedVoicefile": true,
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感度Lv.6後に解放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 18024,
			"title": "「神の目」について…",
			"voicelineType": "3001",
			"description": "この小さなガラス玉は、バルバトス様がクレーを認めてくれたってこと…？うーん…クレーのどこを認めてくれたんだろ？自由なとこ？じゃあ、バルバトス様の期待に応えられるように、クレーはもっと自由にお魚をドカーンしたほうがいいかな…？",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感度Lv.4後に解放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18025,
			"title": "シェアしたいこと…",
			"voicelineType": "3002",
			"description": "騎士団の壁を壊しちゃだめだってクレーは知ってるの。壊せばすぐ反省室から出られるけど、もう明日の太陽は拝めないかもってガイアお兄ちゃんが言ってた…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_share"
		},
		{
			"voicelineId": 18026,
			"title": "興味のあること…",
			"voicelineType": "3003",
			"description": "トカゲはどれも模様が違うよ。一部のトカゲのしっぽは乾燥させて粉にすると、爆薬の材料に使えるの。これはアルベドお兄ちゃんが教えてくれたんだ。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_pref_gift"
		},
		{
			"voicelineId": 18027,
			"title": "レザーについて…",
			"voicelineType": "4001",
			"description": "友達だよ！レザーが頑張って危険からクレーを守ってくれる姿は安心できるんだ！…うーん…危険は大体クレーが持ってくるんだけどね…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_razor_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感度Lv.4後に解放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18028,
			"title": "両親について…",
			"voicelineType": "4002",
			"description": "パパもママも大陸全土で有名な旅人だよ！ママの書いた『テイワット観光ガイド』のレビュー欄は、たくさんのレストランと宿屋が気にしているらしいの。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_parent_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感度Lv.4後に解放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18029,
			"title": "ガイアについて…",
			"voicelineType": "4003",
			"description": "ガイアお兄ちゃんはいい人だよ！『騎士団で生きるための心得』はガイアお兄ちゃんが作ってくれたの。クレーがトラブルを起こした時も、よく内緒にしてくれるんだ。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_kaeya_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感度Lv.4後に解放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18030,
			"title": "アンバーについて…",
			"voicelineType": "4004",
			"description": "アンバーお姉ちゃんはいい人だよ！ウサギ伯爵はモフモフしてて、ドカーンするから最高なんだ！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_ambor_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感度Lv.4後に解放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
