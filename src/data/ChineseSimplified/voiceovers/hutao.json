{
	"id": 10000046,
	"name": "Hu Tao",
	"friendLines": [
		{
			"voicelineId": 36009,
			"title": "初次见面…",
			"voicelineType": "1000",
			"description": "唷，找本堂主有何贵干呀？嗯？你不知道吗，往生堂第七十七代堂主就是胡桃我啦！嘶，不过瞧你的模样，容光焕发，身体健康，嗯…想必是为了工作以外的事来找我，对吧？",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_draw_appear"
		},
		{
			"voicelineId": 36010,
			"title": "闲聊·往生堂",
			"voicelineType": "1101",
			"description": "不如去我那儿喝杯茶？嘿~",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_explore_idle_01"
		},
		{
			"voicelineId": 36011,
			"title": "闲聊·客户",
			"voicelineType": "1102",
			"description": "一个客户，两个客户，三个客户…",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_explore_idle_02"
		},
		{
			"voicelineId": 36012,
			"title": "闲聊·偷懒",
			"voicelineType": "1103",
			"description": "咳咳…太阳出来我晒太阳，月亮出来我晒月亮咯~",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_explore_idle_03"
		},
		{
			"voicelineId": 36013,
			"title": "下雨的时候…",
			"voicelineType": "1201",
			"description": "哎呀…麻烦喽。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_heavyRain_01"
		},
		{
			"voicelineId": 36014,
			"title": "打雷的时候…",
			"voicelineType": "1202",
			"description": "打雷啦，起身啦。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_thunder_01"
		},
		{
			"voicelineId": 36015,
			"title": "下雪的时候…",
			"voicelineType": "1203",
			"description": "哇，是雪！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_snowy_01"
		},
		{
			"voicelineId": 36016,
			"title": "起风的时候…",
			"voicelineType": "1204",
			"description": "嗯，真舒服。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_breeze_01"
		},
		{
			"voicelineId": 36017,
			"title": "刮大风了…",
			"voicelineType": "1205",
			"description": "欸？…唉，帽子都要吹飞了。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_weather_gale_01"
		},
		{
			"voicelineId": 36018,
			"title": "早上好…",
			"voicelineType": "1001",
			"description": "嗯~早起身体好，晚睡人会飘。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_greetingMorning"
		},
		{
			"voicelineId": 36019,
			"title": "中午好…",
			"voicelineType": "1002",
			"description": "哟！中午好呀，吃了吗？",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_greetingNoon"
		},
		{
			"voicelineId": 36020,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "嘻嘻，月亮出来喽~咱也出门吧。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_greetingNight"
		},
		{
			"voicelineId": 36021,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "哦，困了吗？那你好好休息，我一个人四处转转。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_greetingNight2"
		},
		{
			"voicelineId": 36022,
			"title": "关于胡桃自己·名字",
			"voicelineType": "2001",
			"description": "胡桃的胡是胡吃海喝的胡，胡桃的桃却不是淘气的淘！嘿嘿…不、不好笑吗？",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_idle"
		},
		{
			"voicelineId": 36023,
			"title": "关于胡桃自己·拿手好戏",
			"voicelineType": "2002",
			"description": "变个戏法给你看，瞧好了哦…火！然后…嚯！火蝴蝶！呼，飞啦——哈哈。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_idle2"
		},
		{
			"voicelineId": 36024,
			"title": "关于我们·好奇心",
			"voicelineType": "2003",
			"description": "你到处旅行，一定见多识广。有空也给我讲讲你自己的故事吧。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_close"
		},
		{
			"voicelineId": 36025,
			"title": "关于我们·夜晚",
			"voicelineType": "2004",
			"description": "白天你尽管到处跑，晚上可得小心点。我不在的时候，务必谨慎行动。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_close1",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 36026,
			"title": "关于我们·帮手",
			"voicelineType": "2005",
			"description": "需要帮手吗？需要帮手吗？我来啦！若你需要帮助，胡桃我定当全力以赴，绝不推辞。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_close2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 36027,
			"title": "关于我们·战斗",
			"voicelineType": "2006",
			"description": "战斗是件麻烦事，对我来说，它不是目的而是手段。用手段来达到目的，用战斗来维护不愿让步的东西。就这方面而言，我和你是一样的。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_close3",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 36028,
			"title": "关于「神之眼」…",
			"voicelineType": "3001",
			"description": "「神之眼」…「神之眼」？唔，这东西，随缘就好。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36029,
			"title": "有什么想要分享…",
			"voicelineType": "3002",
			"description": "受人之托，忠人之事。我们往生堂性质特殊，肩负着双倍责任，所以，一定要让两个世界的人都满意才行。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_share_01"
		},
		{
			"voicelineId": 36030,
			"title": "感兴趣的见闻…",
			"voicelineType": "3003",
			"description": "有些花草只长在阴气重月光多的地方，你见过吗？没有？那下次带你见识见识。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_pref_gift"
		},
		{
			"voicelineId": 36031,
			"title": "关于钟离…",
			"voicelineType": "4001",
			"description": "钟离长得那么年轻，性格倒像是个老古董，什么都懂，却什么都不关心，他该不会…？嘻嘻…\n唉，算了，随他去吧。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_zhongli_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36032,
			"title": "关于七七…",
			"voicelineType": "4002",
			"description": "看到七七了吗？快告诉我她在哪，我要把她藏起来，嘿！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_qiqi_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36033,
			"title": "关于白术…",
			"voicelineType": "4003",
			"description": "白术？哼，麻烦的家伙。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_baizhu_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36034,
			"title": "关于凝光…",
			"voicelineType": "4004",
			"description": "天权凝光，富甲一方；明眸善睐，桃羞杏让；目达耳通，百了千当！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_ningguang_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36035,
			"title": "关于北斗…",
			"voicelineType": "4005",
			"description": "北斗姐可是璃月名人，她不认识我没关系，我认识她就行。啊~真想和她交个朋友啊。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_beidou_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36036,
			"title": "关于行秋…",
			"voicelineType": "4006",
			"description": "听说行秋少爷在写书？话本还是怪谈呢？好想把平日遇见的杂事说给他听，写成书一定很好看吧。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_xingqiu_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36037,
			"title": "关于重云…",
			"voicelineType": "4007",
			"description": "纯阳正气，阴阳合一？啧啧，嘶…没想到世上真有这种体质的人。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_chongyun_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36038,
			"title": "关于魈…",
			"voicelineType": "4008",
			"description": "仙家的事，我怎么知道？哎呀呀，天机不可泄露。你就别问我啦。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_xiao_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36039,
			"title": "关于刻晴…",
			"voicelineType": "4009",
			"description": "「玉衡」刻晴啊，嘶…是个很难打交道的人呢。嘿嘿，瞧那一本正经的样子，真想逗逗她。哎呦，她应该不会用剑敲我头吧？不会的吧？",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_keqing_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36040,
			"title": "关于香菱…",
			"voicelineType": "4010",
			"description": "香菱可好玩了，一碰就跳，特别容易被唬住。做我们这行，最喜欢这种人喽。不过也得注意分寸，毕竟，锅巴会替香菱出头呢。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_xiangling_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36041,
			"title": "关于云堇…",
			"voicelineType": "4011",
			"description": "云堇她在正式场合很优雅，私底下却非常容易亲近哦！上次我跟她打了个赌，输的人要用自己习惯的唱法唱对方填的词。哎呀呀，戏腔版的丘丘谣，你真该听一次！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_yunjin_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36042,
			"title": "关于申鹤…",
			"voicelineType": "4012",
			"description": "她可真是个有意思的人呐，跟别人都不一样。你说，要是我从现在开始天天吃仙草、喝山露，会不会也变成她那么不染红尘的样子？嘿嘿…",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_teammate_shenhe_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36043,
			"title": "想要了解胡桃·其一",
			"voicelineType": "5001",
			"description": "大丘丘病了，二丘丘瞧；三丘丘采药，四丘丘熬；五丘丘死了，六丘丘抬~\n欸…呼。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_01"
		},
		{
			"voicelineId": 36044,
			"title": "想要了解胡桃·其二",
			"voicelineType": "5002",
			"description": "小巷派暗黑打油诗人，正是本人！",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到3后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 36045,
			"title": "想要了解胡桃·其三",
			"voicelineType": "5003",
			"description": "若要我说，往生堂的工作既不困难也不可怕，只是麻烦而已。可惜旁人理解不了，见到咱们夜里出工，总是有些惧怕。唉，是他们不懂啊。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 36046,
			"title": "想要了解胡桃·其四",
			"voicelineType": "5004",
			"description": "我是「往生堂」第七十七代堂主，我爷爷是第七十五代。我经手的第一份工作，就是替爷爷办葬礼。是不是很意外啊？哎呀，不知不觉就在这行做了好些年，时间过得可真快。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到5后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				}
			]
		},
		{
			"voicelineId": 36047,
			"title": "想要了解胡桃·其五",
			"voicelineType": "5005",
			"description": "阴阳有序，命运无常。死亡难以预测，却也有它的规矩。记住了，无论何时何地，无论有何原因，人都不该轻易挑衅「死」之一事。唯有认识它、尊重它，才能明白活着的价值。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_friendship_05",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 36048,
			"title": "胡桃的爱好…",
			"voicelineType": "6001",
			"description": "天清海阔，皓月凌空，此情此景，正适合作诗一首。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_pref_hobby"
		},
		{
			"voicelineId": 36049,
			"title": "胡桃的烦恼…",
			"voicelineType": "6002",
			"description": "唉…真想找些新乐子，无所事事比死亡可怕多了。",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_dialog_annoyed"
		},
		{
			"voicelineId": 36050,
			"title": "喜欢的食物…",
			"voicelineType": "6003",
			"description": "♪啦啦啦啦~吃水煮鱼配虾饺啦~",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_pref_foodLike"
		},
		{
			"voicelineId": 36051,
			"title": "讨厌的食物…",
			"voicelineType": "6004",
			"description": "清心炒史莱姆凝液…呕…这是香菱对我的复仇吗？",
			"voicefile": "VO_friendship/VO_hutao/vo_hutao_pref_foodDislike"
