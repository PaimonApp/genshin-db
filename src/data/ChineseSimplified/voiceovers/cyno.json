{
	"id": 10000071,
	"name": "Cyno",
	"friendLines": [
		{
			"voicelineId": 71000,
			"title": "初次见面·过往",
			"voicelineType": "1000",
			"description": "我是赛诺，曾经的「大风纪官」。如今我正在以自己的力量审判罪恶，若你同样心系规则与正义，我可以助你一臂之力。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_draw_appear_01"
		},
		{
			"voicelineId": 71001,
			"title": "初次见面·如今",
			"voicelineType": "1006",
			"description": "我是赛诺，教令院的「大风纪官」。维护规则、审判罪恶是我的职责，今后的旅途中，我会保护好你的安全。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_draw_appear_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "完成「虚空鼓动，劫火高扬」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						3026
					]
				}
			]
		},
		{
			"voicelineId": 71002,
			"title": "闲聊·称重",
			"voicelineType": "1101",
			"description": "善与恶，最终都会被称量。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_character_idle_01"
		},
		{
			"voicelineId": 71003,
			"title": "闲聊·牌运",
			"voicelineType": "1102",
			"description": "运势会左右某一场对决的胜负，但不会一直影响下去。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_character_idle_02"
		},
		{
			"voicelineId": 71004,
			"title": "闲聊·疑惑",
			"voicelineType": "1103",
			"description": "偶尔我也想缓解一下气氛，但我其实分不太清什么笑话能够引人发笑。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_character_idle_03"
		},
		{
			"voicelineId": 71005,
			"title": "下雨的时候…",
			"voicelineType": "1201",
			"description": "又下雨了…",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_weather_heavyRain_01"
		},
		{
			"voicelineId": 71006,
			"title": "打雷的时候…",
			"voicelineType": "1202",
			"description": "在一些传说中，雷电是神明对人间降下的审判。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_weather_thunder_01"
		},
		{
			"voicelineId": 71007,
			"title": "下雪的时候…",
			"voicelineType": "1203",
			"description": "我很喜欢下雪后的世界，安静却并非一片死寂。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_weather_snowy_01"
		},
		{
			"voicelineId": 71008,
			"title": "阳光很好…",
			"voicelineType": "1204",
			"description": "阳光终会照进每一个角落。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_weather_sunny_01"
		},
		{
			"voicelineId": 71009,
			"title": "刮大风了…",
			"voicelineType": "1205",
			"description": "好大的风…当心。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_weather_gale_01"
		},
		{
			"voicelineId": 71010,
			"title": "在沙漠的时候…",
			"voicelineType": "1206",
			"description": "熟悉的风景…",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_weather_desert_01"
		},
		{
			"voicelineId": 71011,
			"title": "早上好…",
			"voicelineType": "1001",
			"description": "打起精神，该出发了。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_greetingMorning"
		},
		{
			"voicelineId": 71012,
			"title": "中午好…",
			"voicelineType": "1002",
			"description": "需要我准备午饭吗？事先说好，我可不保证好吃。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_greetingNoon"
		},
		{
			"voicelineId": 71013,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "还记得我跟你说过的话吗？夜晚总是伴随着危险，无论做什么都不要放松警惕。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_greetingNight"
		},
		{
			"voicelineId": 71014,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "晚安。一些罪犯在附近活动，我去处理。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_greetingNight2"
		},
		{
			"voicelineId": 71015,
			"title": "关于赛诺自己·冷笑话",
			"voicelineType": "2001",
			"description": "有一个旅人在沙漠中口渴无比，于是祈祷自己遇到一只水史莱姆，没想到一只饿着肚子的水史莱姆也在祈祷自己遇见落单的旅人…嗯？你那是什么表情，不好笑吗？",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_idle"
		},
		{
			"voicelineId": 71016,
			"title": "关于赛诺自己·散步",
			"voicelineType": "2005",
			"description": "我偶尔会去夜晚的沙漠散步，寒冷而干涩的风让人头脑清醒。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_idle2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71017,
			"title": "关于我们·决斗",
			"voicelineType": "2003",
			"description": "要打一把「七圣召唤」吗？给你看看我的新卡组。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_close"
		},
		{
			"voicelineId": 71018,
			"title": "关于我们·此刻",
			"voicelineType": "2002",
			"description": "过去无法更改，未来无法预料，所以我很珍惜眼前的时光。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_close1",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 71019,
			"title": "关于「神之眼」…",
			"voicelineType": "3001",
			"description": "凭依在我身上的非人之物也好，将视线投射于我的魔神也罢，我受神明青睐，但我无意为此而改变自己的道义。我只尽我应尽之责，做我应做之事。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71020,
			"title": "有什么想要分享·求知欲",
			"voicelineType": "3002",
			"description": "「欲望泛滥就会引发灾害」，这似乎已经是一种共识。然而提及「求知欲」的时候，人们却往往只愿意谈论它的正面作用…在我看来，求知欲也是欲望的一种，必须加以控制。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_share_01"
		},
		{
			"voicelineId": 71021,
			"title": "有什么想要分享·敬畏心",
			"voicelineType": "3005",
			"description": "在执行任务时，我的任何一个判断都可能会改变他人的命运。所以我必须要求自己时刻对「风纪官」这个职位保持敬畏——就像我敬畏知识与力量一样。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_dialog_share_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71022,
			"title": "感兴趣的见闻…",
			"voicelineType": "3003",
			"description": "我喜欢生长在沙漠中的那些灌木。沙漠象征着归寂，一切文明都会被风沙掩埋。但就在这片荒凉的土地上，却能见到生命不断萌发的迹象，有时连我也会被它们的坚韧震撼。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_pref_gift_01"
		},
		{
			"voicelineId": 71023,
			"title": "关于提纳里·知识",
			"voicelineType": "4001",
			"description": "提纳里和我见过的其他学者不同。他对知识同样有着很深的执着，却始终不会逾越那道红线。有些学者会将自己视为「实验室中的神明」，但提纳里不会，他一直平等地尊重每一种生命。听说他拒绝了教令院的聘请，我也站在他这一边。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_teammate_tighnari_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71024,
			"title": "关于提纳里·笑话",
			"voicelineType": "4002",
			"description": "提纳里曾劝我不要在饭桌上讲笑话，说是根本笑不出来。我不以为然，连讲了三个压箱底的笑话，并逐一解释了笑点…然后我的饭菜凉了。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_teammate_tighnari_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71025,
			"title": "关于柯莱…",
			"voicelineType": "4003",
			"description": "我曾经帮她封印了体内的魔神残骸，但封印的过程给她带来了很大的痛苦。那种程度的痛苦可能比死亡更让人害怕，她能够坚持下来，很了不起。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_teammate_collei_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71026,
			"title": "关于艾尔海森…",
			"voicelineType": "4004",
			"description": "我不太喜欢过于聪明的人，他们总是自认为能掌控一切，从而经常在危险的边缘游走。不过艾尔海森还算稳定，在绝大部分场合，他确实有掌控局面、化险为夷的能力。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_teammate_alhaitham_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「赤土之王与三朝圣者」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						3021
					]
				}
			]
		},
		{
			"voicelineId": 71027,
			"title": "关于卡维…",
			"voicelineType": "4005",
			"description": "提纳里和柯莱没有亲口说出来，但他们之间的眼神交流已经说明了一切。卡维分享的亲身经历比我准备的笑话有趣多了。我的看法？我觉得他们会为这种事情发笑，反倒是挺好笑的。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_teammate_kaveh_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71028,
			"title": "关于纳西妲…",
			"voicelineType": "4006",
			"description": "评价神明…这在我看来是略微有些僭越的事情。不过在同行的那段时间里，我也亲眼见证了她的智慧、勇气与决心。相较于那些只靠臆想就对她指指点点的贤者，我应该更有资格下这个结论：她是一位优秀的、值得尊敬的神明。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_teammate_nahida_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「虚空鼓动，劫火高扬」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						3026
					]
				}
			]
		},
		{
			"voicelineId": 71029,
			"title": "关于妮露…",
			"voicelineType": "4007",
			"description": "我能从她身上感到对舞蹈的热情与执念，这份执念非常坚定，不会轻易改变。希望以后，她也可以用自己热爱的舞蹈为他人带去快乐。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_teammate_nilou_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「虚空鼓动，劫火高扬」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						3026
					]
				}
			]
		},
		{
			"voicelineId": 71030,
			"title": "关于珐露珊…",
			"voicelineType": "4008",
			"description": "我曾仔细翻阅过记载珐露珊前辈失踪的案卷，那时候我怀疑她进行了某种危险的研究…不过与她见面之后，我就彻底打消了这个念头。虽然相处起来有些困难，不过她就像一位喜欢絮絮叨叨的长辈，心里会为他人着想，不会做什么出格的事情。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_teammate_faruzan_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71031,
			"title": "关于丽莎…",
			"voicelineType": "4009",
			"description": "丽莎和我曾向同一位贤者求学，我自然知道她的事。身为大风纪官，我也见过许多过分追求知识而引发的悲剧，所以能理解她的选择。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_teammate_lisa_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71032,
			"title": "想要了解赛诺·其一",
			"voicelineType": "5001",
			"description": "你问关于我的事？也是，毕竟在很多人眼中我是一个无法忽视的「麻烦」…不用担心，只要你不做什么危险的事情，我就不会找你的麻烦。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_friendship_01"
		},
		{
			"voicelineId": 71033,
			"title": "想要了解赛诺·其二",
			"voicelineType": "5002",
			"description": "以此身为容器，可以令非人之物的力量显现。这既得益于先天的体质，也是一种后天的交易。没什么可避讳的，我以风纪官的身份称量世间的罪恶，自然也要把我自己的灵魂放到审判之秤上一同称量。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到3后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 71034,
			"title": "想要了解赛诺·其三",
			"voicelineType": "5003",
			"description": "比起「为什么做」，我更看重「做了什么」。因为这个世界上有成千上万种理由，有时你听到那些理由，会觉得做了错事的人情有可原，甚至觉得旁人对他们太过苛责。劣势方总是容易引起别人的同情，但审判者却要摒弃这种情绪。情有可原，但罪无可赦。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 71035,
			"title": "想要了解赛诺·其四",
			"voicelineType": "5004",
			"description": "在幽暗的无知之海上，偶尔会泛起微光，吸引着不明真相的人靠近。常有人视其为灯塔，却看不到漆黑的海水中隐藏的巨大危险。将久居在黑暗中的人带离光明的指引，本身是一件残酷的事情，但我不得不这么做——直到未来有一天，我们能够战胜海水中潜藏的敌人为止。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到5后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				}
			]
		},
		{
			"voicelineId": 71036,
			"title": "想要了解赛诺·其五",
			"voicelineType": "5005",
			"description": "我判断有价值的信息，已经如数分享了，你还在期待听我说什么？哈…你的好奇心真是不简单，那就由你提问吧。习惯了审问别人之后，这种被人询问的感觉…也算新奇。",
			"voicefile": "VO_friendship/VO_cyno/vo_cyno_friendship_05",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
