{
	"id": 10000063,
	"name": "Shenhe",
	"friendLines": [
		{
			"voicelineId": 63000,
			"title": "初次见面…",
			"voicelineType": "1000",
			"description": "我名申鹤，命格孤煞，易伤身边人，仙人叮嘱要以此红绳锁魂，控制杀心。若你不畏惧与我同行…很好，就请伸出手来吧。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_draw_appear"
		},
		{
			"voicelineId": 63001,
			"title": "闲聊·归宿",
			"voicelineType": "1101",
			"description": "人间的归宿是一种奢望，我早已习惯跟山川与鸟兽作伴了。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_character_idle_01"
		},
		{
			"voicelineId": 63002,
			"title": "闲聊·要诀",
			"voicelineType": "1102",
			"description": "回向正道，保身护命…",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_character_idle_02"
		},
		{
			"voicelineId": 63003,
			"title": "闲聊·刀枪无眼",
			"voicelineType": "1103",
			"description": "刀枪无眼，红绳的力量也有极限，所以我才不喜欢人多的地方。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_character_idle_03"
		},
		{
			"voicelineId": 63004,
			"title": "下雨的时候…",
			"voicelineType": "1201",
			"description": "按照凡人的做法，这种时候，是否应该…撑伞？",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_weather_heavyRain_01"
		},
		{
			"voicelineId": 63005,
			"title": "雨过天晴…",
			"voicelineType": "1202",
			"description": "天气变好，我也很开心。只希望没有人来打扰…如果有，就让他们消失好了。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_weather_afterRain_01"
		},
		{
			"voicelineId": 63006,
			"title": "打雷的时候…",
			"voicelineType": "1203",
			"description": "把魔物冻在原地，雷劈下来就能瞬间解决它们，还不会让衣服沾上尘土。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_weather_thunder_01"
		},
		{
			"voicelineId": 63007,
			"title": "刮大风了…",
			"voicelineType": "1204",
			"description": "嗯…师父最喜欢庆云顶上的罡风。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_weather_gale_01"
		},
		{
			"voicelineId": 63008,
			"title": "早上好…",
			"voicelineType": "1001",
			"description": "早安。晨露很甜，要分一点给你吗？",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_greetingMorning"
		},
		{
			"voicelineId": 63009,
			"title": "中午好…",
			"voicelineType": "1002",
			"description": "若是午后困乏，不妨试试打坐养神，很有效果。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_greetingNoon"
		},
		{
			"voicelineId": 63010,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "不知道你是喜欢人间的灯火，还是山林的月光？",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_greetingNight"
		},
		{
			"voicelineId": 63011,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "放心休息吧，我睡眠很浅，有什么动静，我会替你查看的。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_greetingNight2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63012,
			"title": "关于申鹤自己·本能",
			"voicelineType": "2001",
			"description": "磨砺自身，修炼方术，都是本能使然，想要保全自己而已。我没有兼济苍生的胸怀，惩奸除恶只是顺手，不必感谢我。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_idle"
		},
		{
			"voicelineId": 63013,
			"title": "关于申鹤自己·异类",
			"voicelineType": "2005",
			"description": "受我恩惠的人叫我仙人，被我惩罚的人喊我魔女，呵…不管是仙是魔，终究都是跟凡人不同的异类罢了…",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_idle2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63014,
			"title": "关于我们·待人接物",
			"voicelineType": "2003",
			"description": "我很少下山，不懂得多少待人接物的道理。发生冲突的时候，我一律按自己的方式处理。但，如果你觉得不妥，都请告诉我，即使不理解，我也会听你的。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_close"
		},
		{
			"voicelineId": 63015,
			"title": "关于我们·红绳",
			"voicelineType": "2002",
			"description": "师父赐我红绳用以压制心性，但这些日子你我同修，我才真正领悟到要诀，力量收放也更自在…你才是真正能锁住我的红绳。若你懂得这份心情，就请留在我身旁。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_close1",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				},
				{
					"unlockText": "完成「风起鹤归」剧情",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						1101505
					]
				}
			]
		},
		{
			"voicelineId": 63016,
			"title": "关于「神之眼」…",
			"voicelineType": "3001",
			"description": "原来这颗闪耀的石头叫做「神之眼」？呵…有缘既得，无缘便失。日后倘若缘分消散，它也会熄灭吧。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63017,
			"title": "有什么想要分享…",
			"voicelineType": "3002",
			"description": "邪祟易驱，心魔难除。若自身无法放下执念，仙法与术式都无从挽回。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_share_01"
		},
		{
			"voicelineId": 63018,
			"title": "感兴趣的见闻…",
			"voicelineType": "3003",
			"description": "为什么说「初雪多浪漫」？那处山巅日日下雪，何日才算是初雪？我不理解，请你告诉我。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_pref_gift_01"
		},
		{
			"voicelineId": 63019,
			"title": "关于凝光…",
			"voicelineType": "4001",
			"description": "人们所谓的财富，对我而言没什么用。但到了凝光小姐手里，却能解决连刀枪都无法处理的难题。她能掌握人间规则，洞察人心…师父说，这些连仙人都无法做到。我很佩服她。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_ningguang_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63020,
			"title": "关于北斗…",
			"voicelineType": "4002",
			"description": "与她相处不用讲究礼节，交谈时也不用揣测深意，让我轻松了不少。只是…她身边总是有人，听说船上还有诸多船员…我需要跟你一起，才能应对人多的场合。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_beidou_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63021,
			"title": "关于云堇…",
			"voicelineType": "4003",
			"description": "云堇唱的…叫做「璃月戏」吧？我很喜欢这种形式。不必亲见其面，就能了解他人的事迹。如果将来她把你唱进戏里，那一定会是非常精彩的故事，我很期待。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_yunjin_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63022,
			"title": "关于甘雨…",
			"voicelineType": "4004",
			"description": "我从师父那里听过许多有关她的事…她原先在我心中的形象，和实际差距还是挺大的。放心，我不会说出去。说起来，璃月港的工作竟这般繁忙…看来融入人间，也要付出一些代价。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_ganyu_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63023,
			"title": "关于重云…",
			"voicelineType": "4005",
			"description": "真没想到我在这世上…还有「亲缘」。他若是有什么修行上的困惑，我愿帮忙解答。不过，得请他上山来。他们一家太过热情，我不知道该如何回绝才符合礼数…",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_chongyun_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63024,
			"title": "关于行秋…",
			"voicelineType": "4006",
			"description": "我曾见到他凑在你耳旁不知说些什么。若他敢威胁你，我一定会将他…嗯？只是朋友间的玩闹话？当真如此吗…",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_xingqiu_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63025,
			"title": "关于钟离…",
			"voicelineType": "4007",
			"description": "那位先生自称是凡人，却与身为仙人的师父很熟络。师父曾说，如果碰见他在仙府前的石凳上饮酒，莫要打扰，让他静静独坐一会。我照做了。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_zhongli_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63026,
			"title": "关于白术…",
			"voicelineType": "4008",
			"description": "他的药草品质很好，为人也随和周到。相比起来，脖子上的小蛇倒是口无遮拦…放心，我没有动手。我还不至于和一条蛇置气。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_baizhu_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63027,
			"title": "关于胡桃…",
			"voicelineType": "4009",
			"description": "她在附近山林游荡时，经常会唱奇怪的歌。久而久之，我把她的歌断断续续听了个遍。听习惯了也还是觉得奇怪，但，我并不讨厌。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_hutao_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63028,
			"title": "关于魈…",
			"voicelineType": "4010",
			"description": "印象中他一直不苟言笑。近来见他，虽说沉默依旧，但…像是比从前轻松了不少。兴许是结识了哪位良人吧。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_xiao_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「迫近的客星」剧情",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						102510
					]
				}
			]
		},
		{
			"voicelineId": 63072,
			"title": "关于闲云…",
			"voicelineType": "4011",
			"description": "上回，师父带漱玉来万民堂吃饭，漱玉言谈间已有了几分师父的神形…我也是在师父的呵护下如此成长，不知道我的举止，是否也带着些师父的样子呢…",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_teammate_xianyun_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「千里月明」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						11035
					]
				}
			]
		},
		{
			"voicelineId": 63029,
			"title": "想要了解申鹤·其一",
			"voicelineType": "5001",
			"description": "请不必在意人们对我的称呼，我不是仙人，也没什么神通。生于驱魔世家的旁系，长在远离璃月港的山间，只能算是个仙家弟子，仅此而已。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_friendship_01"
		},
		{
			"voicelineId": 63030,
			"title": "想要了解申鹤·其二",
			"voicelineType": "5002",
			"description": "遇到恶意滋事的人，人们似乎都会选择上前说教，我不太明白。明明可以直接将这些拦路碍事、心存歹念的恶人斩草除根，为什么要这般麻烦？",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到3后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 63031,
			"title": "想要了解申鹤·其三",
			"voicelineType": "5003",
			"description": "我师从多位仙家，静心修炼。随着修为提升，他们不再传授我新的术式，而是改教我怎样收敛杀性。别紧张…像平地凿出深坑，树木连根拔起之类的事，只在远离人烟的山林中发生过，从未波及过无辜性命。况且，现在我也已经能控制自我了。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63032,
			"title": "想要了解申鹤·其四",
			"voicelineType": "5004",
			"description": "父亲想用我的血肉换回母亲的生命。自从被他丢在山洞，我一度期盼过他会回来救我。一天、两天、三天…我拼尽全力与魔物厮杀，直到倒下都没能等到他。我不知道，究竟是我命格孤煞才遭人遗弃，还是命途坎坷使得我萌生出这股煞气？",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到5后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				},
				{
					"unlockText": "完成「风起鹤归」剧情",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						1101505
					]
				}
			]
		},
		{
			"voicelineId": 63033,
			"title": "想要了解申鹤·其五",
			"voicelineType": "5005",
			"description": "有一回，我瞒着师父偷偷下山回到村里，却发现原本的村庄荒弃了，父亲早已去世。我突然明白，自己与血亲的缘分彻底断绝了。在乎的往事永无后话。自那日起，我一度舍弃人间种种…直到遇见你。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_friendship_05",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				},
				{
					"unlockText": "完成「风起鹤归」剧情",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						1101505
					]
				}
			]
		},
		{
			"voicelineId": 63034,
			"title": "申鹤的爱好…",
			"voicelineType": "6001",
			"description": "打坐的要点在于「静」，需要摒弃杂念，用身体感受周遭气息，它能让我迅速回归自我。听师父说，修为达到足够境界后，打坐的姿势就不用再拘泥，仙人哪怕站立着都能打坐…我还远远不够。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_pref_hobby"
		},
		{
			"voicelineId": 63035,
			"title": "申鹤的烦恼…",
			"voicelineType": "6002",
			"description": "害怕失去重要之人、想要拯救重要之人…这种心情会将整颗心紧紧揪住，以前从未体会过。这是困扰，但…也是寄托。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_dialog_annoyed",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 63036,
			"title": "喜欢的食物…",
			"voicelineType": "6003",
			"description": "下山期间尝了不少菜，但都不太记得味道了。唯独从前在家吃的那碗面，永远忘不了…不该记住才对，不该记住才对…",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_pref_foodLike"
		},
		{
			"voicelineId": 63037,
			"title": "讨厌的食物…",
			"voicelineType": "6004",
			"description": "药草嚼得久了，只剩苦味与涩味…修行多年，我早已习惯这些清规戒律了。仙人们？「仙人不会被人间俗物阻碍修为，所以可以敞开吃各式菜肴。」…这是师父的原话。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_pref_foodDislike"
		},
		{
			"voicelineId": 63069,
			"title": "收到赠礼·其一",
			"voicelineType": "6007",
			"description": "这般滋味…真的是我能够享受的吗？",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_spice_pref_01"
		},
		{
			"voicelineId": 63070,
			"title": "收到赠礼·其二",
			"voicelineType": "6008",
			"description": "你做的饭菜，确实美味。无法割舍。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_spice_normal_01"
		},
		{
			"voicelineId": 63071,
			"title": "收到赠礼·其三",
			"voicelineType": "6009",
			"description": "也好。这样的味道，我早就熟悉了。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_spice_dislike_01"
		},
		{
			"voicelineId": 63038,
			"title": "生日…",
			"voicelineType": "7001",
			"description": "我听说生辰是个值得铭记的日子，人们会向寿星送上礼品以示庆贺。身外之物，我不太了解。银河夜色，是我在山林中见过最美丽的事物。我将湖面冻成明镜，让浮云和星斗映照在眼前，跟你共贺生辰…希望你喜欢。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_pref_birthday_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"conditionType": "FETTER_COND_PLAYER_BIRTHDAY",
					"paramList": []
				}
			]
		},
		{
			"voicelineId": 63039,
			"title": "突破的感受·起",
			"voicelineType": "8001",
			"description": "我能操控的力量…愈发强大了。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_starUp_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等级「1」阶后开启",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						1
					]
				}
			]
		},
		{
			"voicelineId": 63040,
			"title": "突破的感受·承",
			"voicelineType": "8002",
			"description": "师父若知道了，怕是会让我多系几根红绳吧…",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_starUp_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等级「2」阶后开启",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						2
					]
				}
			]
		},
		{
			"voicelineId": 63041,
			"title": "突破的感受·转",
			"voicelineType": "8003",
			"description": "这段日子，我闭关修炼的时长有所削减，修为却不知不觉地增进…嗯，也许只有跟你一起才能做到。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_starUp_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等级「4」阶后开启",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 63042,
			"title": "突破的感受·合",
			"voicelineType": "8004",
			"description": "「心无杂念，方可超然」，这是我从前所想。经历种种才明白，唯有心存牵挂，才能唤醒守护的欲望，让力量…变得有意义。",
			"voicefile": "VO_friendship/VO_shenhe/vo_shenhe_starUp_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等级「6」阶后开启",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						6
					]
				}
			]
		}
	],
