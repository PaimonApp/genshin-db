{
	"id": 10000083,
	"name": "Lynette",
	"friendLines": [
		{
			"voicelineId": 83000,
			"title": "初次见面…",
			"voicelineType": "1006",
			"description": "初次见面，我是林尼的魔术助手，工作事宜请咨询林尼。观看演出请咨询售票处。找我？…请长话短说。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_draw_appear_01"
		},
		{
			"voicelineId": 83001,
			"title": "闲聊·喝茶",
			"voicelineType": "1101",
			"description": "茶泡好了，接下来可以休息一会儿了。",
			"voicefile": "VO_gameplay/VO_lynette/vo_lynette_character_idle_01"
		},
		{
			"voicelineId": 83002,
			"title": "闲聊·哥哥",
			"voicelineType": "1102",
			"description": "林尼好像又在说什么奇怪的话了…真让人放心不下。",
			"voicefile": "VO_gameplay/VO_lynette/vo_lynette_character_idle_02"
		},
		{
			"voicelineId": 83003,
			"title": "闲聊·发呆",
			"voicelineType": "1103",
			"description": "进入待机模式…希望不要有人来打扰我。",
			"voicefile": "VO_gameplay/VO_lynette/vo_lynette_character_idle_03"
		},
		{
			"voicelineId": 83004,
			"title": "下雨的时候…",
			"voicelineType": "1201",
			"description": "雨天适合发呆…",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_weather_heavyRain_01"
		},
		{
			"voicelineId": 83005,
			"title": "雨过天晴…",
			"voicelineType": "1202",
			"description": "这就天晴了，可惜。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_weather_afterRain_01"
		},
		{
			"voicelineId": 83006,
			"title": "打雷的时候…",
			"voicelineType": "1203",
			"description": "唔…耳朵疼。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_weather_thunder_01"
		},
		{
			"voicelineId": 83007,
			"title": "下雪的时候…",
			"voicelineType": "1204",
			"description": "冷…把你藏到帽子里吧。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_weather_snowy_01"
		},
		{
			"voicelineId": 83008,
			"title": "阳光很好…",
			"voicelineType": "1205",
			"description": "太阳底下没有新鲜事…真是这样吗？",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_weather_sunny_01"
		},
		{
			"voicelineId": 83009,
			"title": "刮大风了…",
			"voicelineType": "1206",
			"description": "风里传来的气息…不，没什么。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_weather_gale_01"
		},
		{
			"voicelineId": 83010,
			"title": "早上好…",
			"voicelineType": "1001",
			"description": "早安…充能尚未完成，即将再次休眠。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_greetingMorning"
		},
		{
			"voicelineId": 83011,
			"title": "中午好…",
			"voicelineType": "1002",
			"description": "我习惯工作之前喝一杯茶，能转换心情，也能保持清醒。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_greetingNoon"
		},
		{
			"voicelineId": 83012,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "太阳下山之后，人们内心潜藏的欲望，就会浮上水面…我是说，晚上的演出会很精彩。要给你留票吗？",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_greetingNight"
		},
		{
			"voicelineId": 83013,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "快睡吧，我还要处理白天没做完的事情…（还要把弄坏的吸尘器修好。）",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_greetingNight2"
		},
		{
			"voicelineId": 83014,
			"title": "关于琳妮特自己·魔术",
			"voicelineType": "2001",
			"description": "表演的时候，我一般担任林尼的助手，因为一直不说话也不做表情，有人以为我是魔术人偶…这种误解能避免不必要的交际，倒也不错。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_idle"
		},
		{
			"voicelineId": 83015,
			"title": "关于琳妮特自己·情报",
			"voicelineType": "2005",
			"description": "沉默有助于集中精神，看清有用情报。曾经来看演出的某位贵客，掏出了不属于他太太的手帕…至于情报的具体用途，嗯…就看那位大人的意思了。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_idle2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 83016,
			"title": "关于我们·倾听",
			"voicelineType": "2003",
			"description": "我不习惯主动表达，但应该算是个不错的听众。如果有什么烦恼，就来找我吧。我会为你保密。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_close",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 83017,
			"title": "关于我们·兴趣",
			"voicelineType": "2002",
			"description": "我曾经以为，金色的「太阳」和夜色的「暗影」，是无法互相理解的，可你身上有着奇特的复杂性，让我产生了不必要的好奇心…今后也让我继续观察你吧。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_close1",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 83018,
			"title": "关于「神之眼」…",
			"voicelineType": "3001",
			"description": "比起武力，我更喜欢用自己的眼睛掌握现状，找到合适的解决方法。不过，拥有力量就会拥有更多底牌，就能在危急情况下保护重要的人。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 83019,
			"title": "有什么想要分享…",
			"voicelineType": "3002",
			"description": "泡茶是一件看似简单实则费心的事情，茶叶的品质、茶水的温度、冲泡的次数，只有所有的变量都控制恰当，才能泡出味道最纯正的茶。要来一杯吗？",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_dialog_share_01"
		},
		{
			"voicelineId": 83020,
			"title": "感兴趣的见闻…",
			"voicelineType": "3003",
			"description": "小动物们也会表达自己的情绪，如果仔细聆听的话就会知道它们现在的状态。对于小猫来说，「喵~」比起「喵喵喵！」，前者心情更好，这时候不管怎么摸它都不会逃走哦。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_pref_gift_01"
		},
		{
			"voicelineId": 83021,
			"title": "关于林尼·默契",
			"voicelineType": "4001",
			"description": "我和林尼一直都待在一起，我们即使不开口也能明白对方的想法，所以单独相处时，他会比平时要安静。你问我他说的哪些是真话，哪些是假话？其实我都知道，但这是最高保密事项。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_lyney_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 83022,
			"title": "关于林尼·担忧",
			"voicelineType": "4002",
			"description": "林尼平时总是自信不疑，做起事来游刃有余，或者说，他让所有人都以为他一直如此，所以在遇到麻烦的时候，他也总会想要一个人完美地解决…如果遇到这种情况，还请多多关照他吧。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_lyney_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 83023,
			"title": "关于菲米尼…",
			"voicelineType": "4003",
			"description": "他是个细腻温柔的孩子，却有些喜欢妄自菲薄…或许是和之前的经历有关吧。有时候他给我的感觉就像是林尼的反面…既然两边都是我的家人，意见不合时，我会负责弥合分歧。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_freminet_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 83024,
			"title": "关于「父亲」…",
			"voicelineType": "4004",
			"description": "「父亲」表达爱意的方式…不一定能被每个人接受，但就是因为有「父亲」在，家…才得以为家。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_arlecchino_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「仿若无因飘落的轻雨」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						4002
					]
				}
			]
		},
		{
			"voicelineId": 83025,
			"title": "关于那维莱特…",
			"voicelineType": "4005",
			"description": "最高审判官总给我一种奇怪的疏离感，明明言行谈吐都很谦和…还是说，他也不怎么喜欢跟人打交道呢？不过他和美露莘们说话的时候，倒是很亲切。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_neuvillette_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 83026,
			"title": "关于芙宁娜…",
			"voicelineType": "4006",
			"description": "芙宁娜大人的行为太难解读了，也观察不出什么有用的情报，总觉得她一举一动像是在扮演不同的人…不愧是神明，不能用普通人的角度来揣测。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_furina_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 83027,
			"title": "关于娜维娅…",
			"voicelineType": "4007",
			"description": "虽然是「刺玫会」的新会长，作风却像邻居家爱管闲事的大姐姐呢。要是「家」里真有这么一位姐姐就好了…啊，这话好像不太合适…",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_navia_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「仿若无因飘落的轻雨」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						4002
					]
				}
			]
		},
		{
			"voicelineId": 83028,
			"title": "关于克洛琳德…",
			"voicelineType": "4008",
			"description": "我曾经奉命观摩过她的决斗，仅仅是从旁观察，就能感觉到令人害怕的寒意…真不知道在决斗场上直面她会是什么感觉…嗯，希望这辈子都不要知道。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_clorinde_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「仿若无因飘落的轻雨」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						4002
					]
				}
			]
		},
		{
			"voicelineId": 83029,
			"title": "关于千织…",
			"voicelineType": "4009",
			"description": "我跟林尼的演出服都是请她定制的。有一次林尼用小刀表演魔术，不小心划到了袖口，后来被她数落了好久…真可怕。幸好每次去她店里，都可以躲到林尼背后。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_chiori_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 83030,
			"title": "关于夏洛蒂",
			"voicelineType": "4010",
			"description": "呃…我后来也被她抓着采访过几次。明明被林尼拒绝了那么多次，真是毅力可嘉…可惜她准备了这么多问题，我一个也没回答。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_charlotte_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「仿若无因飘落的轻雨」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						4002
					]
				}
			]
		},
		{
			"voicelineId": 83031,
			"title": "关于艾梅莉埃…",
			"voicelineType": "4011",
			"description": "这位香水设计师小姐的作品热门到需要凌晨排队抢购。但我对香水的味道太敏感，打出喷嚏来就太失礼了…所以只能绕着走。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_teammate_emilie_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 83032,
			"title": "想要了解琳妮特·其一",
			"voicelineType": "5001",
			"description": "如果要解释魔术，就会失去看表演的乐趣，人际关系也是这样，想了解我的话，不妨一点点收集信息，拼凑答案吧。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_friendship_01"
		},
		{
			"voicelineId": 83033,
			"title": "想要了解琳妮特·其二",
			"voicelineType": "5002",
			"description": "人是很复杂的生物，即便不是有意欺瞒，也会不自觉地隐藏自己的真实一面。虽然我总能注意到一些「多余」的细节，但除非必要情况，我不会拆穿别人的伪装。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到3后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 83034,
			"title": "想要了解琳妮特·其三",
			"voicelineType": "5003",
			"description": "魔术曾经是我和林尼的谋生手段，现在更多是一种便利的社会身份，以魔术表演的名义，我们就能融入这座城市，成为家的「眼睛」。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「仿若无因飘落的轻雨」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						4002
					]
				}
			]
		},
		{
			"voicelineId": 83035,
			"title": "想要了解琳妮特·其四",
			"voicelineType": "5004",
			"description": "在来到新家之前，我和林尼一直到处流浪，经历了许多艰难和危险的状况。从那时起，我便习惯时刻注意一切危险的迹象，也逐渐学会解读他人的所思所想。好在，如今我们拥有了一个安身之所。",
			"voicefile": "VO_friendship/VO_lynette/vo_lynette_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到5后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				},
				{
					"unlockText": "完成「仿若无因飘落的轻雨」剧情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						4002
					]
				}
			]
		},
		{
			"voicelineId": 83036,
