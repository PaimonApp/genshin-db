{
	"id": 10000029,
	"name": "Klee",
	"friendLines": [
		{
			"voicelineId": 18009,
			"title": "初次见面…",
			"voicelineType": "1000",
			"description": "西风骑士团，「火花骑士」，可莉，前来报到！…呃——后面该说什么词来着？可莉背不下来啦…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_draw_appear"
		},
		{
			"voicelineId": 18010,
			"title": "闲聊·收获",
			"voicelineType": "1101",
			"description": "可莉今天又勇敢地抓到了花纹奇怪的蜥蜴！从没见过这种图案，你要看看吗？",
			"voicefile": "VO_friendship/VO_klee/vo_klee_explore_idle_01"
		},
		{
			"voicelineId": 18011,
			"title": "闲聊·冒险",
			"voicelineType": "1102",
			"description": "要和可莉一起去炸鱼吗？虽然被抓住就是一整天的禁闭，但鱼很好吃，所以值得！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_explore_idle_02"
		},
		{
			"voicelineId": 18012,
			"title": "闲聊·守则",
			"voicelineType": "1103",
			"description": "「城里放炮禁闭室报到」、「炸弹伤人琴找上门」、「放火烧山可莉完蛋」——这就是可莉在骑士团的生存守则。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_explore_idle_03"
		},
		{
			"voicelineId": 18013,
			"title": "下雨的时候…",
			"voicelineType": "1201",
			"description": "哼哼哼，这次的炸弹可是防水的。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_weather_heavyRain_01"
		},
		{
			"voicelineId": 18014,
			"title": "打雷的时候…",
			"voicelineType": "1202",
			"description": "啊！什么东西爆炸了？！…哦，是打雷啊。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_weather_thunder_01"
		},
		{
			"voicelineId": 18015,
			"title": "刮大风了…",
			"voicelineType": "1203",
			"description": "风神保佑，把可莉的炸弹往合适的地方吹吧。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_weather_gale_01"
		},
		{
			"voicelineId": 18016,
			"title": "早上好…",
			"voicelineType": "1001",
			"description": "早安！带可莉出去玩吧！我们一起来冒险！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_greetingMorning"
		},
		{
			"voicelineId": 18017,
			"title": "中午好…",
			"voicelineType": "1002",
			"description": "午饭时间到了！啊…吃什么呢？我来看看《提瓦特游览指南》…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_greetingNoon"
		},
		{
			"voicelineId": 18018,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "晚上好！拜托你也帮我跟霍夫曼先生说说吧，可莉不是小孩子了，可莉晚上也可以出去玩——带我出去玩——",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_greetingNight"
		},
		{
			"voicelineId": 18019,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "虽然出来玩的时候说不想回家，但天黑以后的旷野，不认识路…拜、拜托你回去的时候，把我也送回家好不好…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_greetingNight2"
		},
		{
			"voicelineId": 18020,
			"title": "关于可莉自己·肇事",
			"voicelineType": "2001",
			"description": "呜…这次居然炸歪了大风车的叶片…真对不起！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_idle"
		},
		{
			"voicelineId": 18021,
			"title": "关于可莉自己·赔偿",
			"voicelineType": "2002",
			"description": "我，我还挺会做手工的，请让我帮你们重新做一个…什么？禁止雇佣童工？呜…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_idle2"
		},
		{
			"voicelineId": 18022,
			"title": "关于我们·骑士",
			"voicelineType": "2003",
			"description": "呼呼…荣誉骑士，可莉作为「火花骑士」，可以说是你在西风骑士团的前辈喔？所以！所以…这次我闯的祸，能不能帮我隐瞒一下…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_close",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "完成「为了没有眼泪的明天」剧情",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						38406
					]
				}
			]
		},
		{
			"voicelineId": 18023,
			"title": "关于我们·玩伴",
			"voicelineType": "2004",
			"description": "不行！才九点，可莉今晚一定要撑到看完马戏团的午夜专场！你是可莉的玩伴吧！那就别想现在就把我送回家，略略略略…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_close1_b",
			"voicefile_male": "VO_friendship/VO_klee/vo_klee_dialog_close1_a",
			"hasGenderedVoicefile": true,
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 18024,
			"title": "关于「神之眼」…",
			"voicelineType": "3001",
			"description": "这个小小的玻璃球，就代表了巴巴托斯大人对可莉的肯定吗…唔…究竟是认可了我的哪一部分呢？是自由吗？那以后要不要更自由地出去炸鱼，来回应巴巴托斯大人的期待呢…？",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18025,
			"title": "有什么想要分享…",
			"voicelineType": "3002",
			"description": "可莉知道，骑士团的墙是不能炸的。虽然打通了能从禁闭室出去很方便，但凯亚哥哥说，这样做了以后，琴团长恐怕会让我再也见不到第二天的太阳…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_share"
		},
		{
			"voicelineId": 18026,
			"title": "感兴趣的见闻…",
			"voicelineType": "3003",
			"description": "每一只蜥蜴的花纹都是不同的，有些蜥蜴的尾巴干燥以后磨成粉，可以当做炸药的材料哦。这是阿贝多哥哥告诉我的事。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_pref_gift"
		},
		{
			"voicelineId": 18027,
			"title": "关于雷泽…",
			"voicelineType": "4001",
			"description": "我们是朋友！雷泽努力对抗危机、保护可莉的样子，让人安心！…唔…虽然基本上都是我把危机带到他身边的…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_razor_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18028,
			"title": "关于父母…",
			"voicelineType": "4002",
			"description": "爸爸和妈妈都是全大陆有名的旅行家！妈妈写的《提瓦特游览指南》，有好多餐馆和旅店都很在意它的权威评价呢。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_parent_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18029,
			"title": "关于凯亚…",
			"voicelineType": "4003",
			"description": "凯亚哥哥是好人！《骑士团生存守则》就是凯亚哥哥帮可莉写的。可莉闯祸的时候，他也经常帮忙遮掩呢。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_kaeya_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18030,
			"title": "关于安柏…",
			"voicelineType": "4004",
			"description": "安柏姐姐是好人！兔兔伯爵摸起来也是毛茸茸的，而且还会爆炸，太棒了！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_ambor_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18031,
			"title": "关于琴…",
			"voicelineType": "4005",
			"description": "琴团长是好人！虽然…有点可怕…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_qin_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18032,
			"title": "关于迪卢克…",
			"voicelineType": "4006",
			"description": "奇怪的大人，一直都是不太开心的样子。唔…为什么不喜欢笑呢？",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_diluc_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18033,
			"title": "关于丽莎…",
			"voicelineType": "4007",
			"description": "丽莎阿…丽莎姐姐是好人！每次可莉靠近她的工坊，她就会给可莉好吃的茶点…欸？说起来我想进她的工坊，好像一开始是想做新的炸药来着…？",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_lisa_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18034,
			"title": "关于班尼特…",
			"voicelineType": "4008",
			"description": "班尼特哥哥是好人！和他一起冒险，可莉总能有更多收获！…虽然班尼特哥哥自己总是什么宝物都找不到，嗯…这是为什么呢？",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_bennett_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18035,
			"title": "关于迪奥娜…",
			"voicelineType": "4009",
			"description": "迪奥娜姐姐是好人！「靠一己之力摧毁蒙德酒业」的想法好有趣，可莉也要用炸弹来帮忙！——呃，虽然不太懂什么叫做「蒙德酒业」，是怪物吗？",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_diona_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18036,
			"title": "关于阿贝多…",
			"voicelineType": "4010",
			"description": "阿贝多哥哥跟可莉有个秘密的约定！要是可莉看到工坊门前挂着「实验进行中」的牌子，就要晚点再去找他。阿贝多哥哥不工作的时候，会很耐心地陪着可莉，所以可莉也会很耐心地等着的！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_albedo_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18037,
			"title": "关于莫娜…",
			"voicelineType": "4011",
			"description": "莫娜姐姐是好人！她每次找阿贝多哥哥和可莉玩的时候，都会说好多听不明白，但是很有趣的东西！可莉喜欢她来玩！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_mona_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18038,
			"title": "关于砂糖…",
			"voicelineType": "4012",
			"description": "砂糖姐姐是好人！她问我，想不想要一个会跑会跳的蹦蹦！嘿嘿嘿，当然想要了，我还想要好几百个，让蒙德的街道上，台阶上，广场上，全是蹦蹦！",
			"voicefile": "VO_friendship/VO_klee/vo_klee_teammate_sucrose_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18039,
			"title": "想要了解可莉·其一",
			"voicelineType": "5001",
			"description": "你好！你是来找可莉玩的吗？",
			"voicefile": "VO_friendship/VO_klee/vo_klee_friendship_01"
		},
		{
			"voicelineId": 18040,
			"title": "想要了解可莉·其二",
			"voicelineType": "5002",
			"description": "是啊，这个炸弹是可莉最新改良的作品！你看我做得好不好？…呃——如果是在什么纵火现场发现的话，那就不是我做的，绝对不是。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到3后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 18041,
			"title": "想要了解可莉·其三",
			"voicelineType": "5003",
			"description": "哎，虽然新炸药的配方，很多都是在被关禁闭的时候想出来的…但如果不被关禁闭的话就更好了…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 18042,
			"title": "想要了解可莉·其四",
			"voicelineType": "5004",
			"description": "这是嘟嘟可，是可莉很久以前就交到的好朋友。要记得它的名字哦，以后别叫它「挂在你包上的玩偶」啦。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到5后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				}
			]
		},
		{
			"voicelineId": 18043,
			"title": "想要了解可莉·其五",
			"voicelineType": "5005",
			"description": "谢谢你总是帮可莉解决麻烦！就用这串「可莉烤鱼」来报答你吧！——呃，其实鱼是直接在湖里就被炸弹烤熟了，不过这件事就和平时一样，假装不知道好了。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_friendship_05",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 18044,
			"title": "可莉的爱好…",
			"voicelineType": "6001",
			"description": "可莉喜欢毛茸茸的东西。比如嘟嘟可、蒲公英，还有雷泽的头发。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_pref_hobby"
		},
		{
			"voicelineId": 18045,
			"title": "可莉的烦恼…",
			"voicelineType": "6002",
			"description": "唉，你是好人，我是坏孩子…等我这次的禁闭结束、好好反省过以后，再来找你带我出去玩…",
			"voicefile": "VO_friendship/VO_klee/vo_klee_dialog_annoyed"
		},
		{
			"voicelineId": 18046,
			"title": "喜欢的食物…",
			"voicelineType": "6003",
			"description": "蒙德的鲈鱼是很好吃的，从果酒湖里抓到的尤其好吃！这可不是瞎说的，连妈妈也认同可莉的想法喔。",
			"voicefile": "VO_friendship/VO_klee/vo_klee_pref_foodLike"
		},
		{
			"voicelineId": 18047,
			"title": "讨厌的食物…",
			"voicelineType": "6004",
			"description": "讨厌蒙德蟹——什么蟹都讨厌——可莉要出去玩——不要坐在餐桌前慢慢剥壳——",
			"voicefile": "VO_friendship/VO_klee/vo_klee_pref_foodDislike"
		},
		{
			"voicelineId": 18077,
			"title": "收到赠礼·其一",
			"voicelineType": "6007",
			"description": "好香好香！可莉已经吃到吃不下啦…",
