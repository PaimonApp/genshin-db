{
	"id": 10000022,
	"name": "Venti",
	"friendLines": [
		{
			"voicelineId": 11100,
			"title": "初次见面…",
			"voicelineType": "1000",
			"description": "哈…睡得好香呀。欸，你好呀，旅行者，我们又见面了。嗯？不记得我了？嘿嘿，那就让我再次加入你的旅程吧。你的诗篇，值得被我这个世上最好的吟游诗人传唱！",
			"voicefile": "VO_friendship/VO_venti/vo_venti_draw_appear"
		},
		{
			"voicelineId": 11101,
			"title": "闲聊·弹琴",
			"voicelineType": "1101",
			"description": "嗯…想接着听下去的话，送我一个苹果吧。",
			"voicefile": "VO_friendship/VO_venti/vo_venti_explore_idle_01"
		},
		{
			"voicelineId": 11102,
			"title": "闲聊·苹果酒",
			"voicelineType": "1102",
			"description": "嗯…好想坐在大树上，手边有喝不完的苹果酒。哎。",
			"voicefile": "VO_friendship/VO_venti/vo_venti_explore_idle_02"
		},
		{
			"voicelineId": 11103,
			"title": "闲聊·诗篇",
			"voicelineType": "1103",
			"description": "动身吧，旅行者。佚失的诗篇，还在等着我们呢。",
			"voicefile": "VO_friendship/VO_venti/vo_venti_explore_idle_03"
		},
		{
			"voicelineId": 11104,
			"title": "下雨的时候…",
			"voicelineType": "1201",
			"description": "来踩小水坑吧！看看谁溅出来的水花更大！",
			"voicefile": "VO_friendship/VO_venti/vo_venti_weather_heavyRain_01"
		},
		{
			"voicelineId": 11105,
			"title": "雨过天晴…",
			"voicelineType": "1202",
			"description": "欸~这就天晴了？我还想多玩一会儿呢。",
			"voicefile": "VO_friendship/VO_venti/vo_venti_weather_afterRain_01"
		},
		{
			"voicelineId": 11106,
			"title": "下雪的时候…",
			"voicelineType": "1203",
			"description": "等雪积起来…我们来打雪仗吧！",
			"voicefile": "VO_friendship/VO_venti/vo_venti_weather_snowy_01"
		},
		{
			"voicelineId": 11107,
			"title": "刮大风了…",
			"voicelineType": "1204",
			"description": "难得呀，要不要去飞一飞？",
			"voicefile": "VO_friendship/VO_venti/vo_venti_weather_gale_01"
		},
		{
			"voicelineId": 11108,
			"title": "早上好…",
			"voicelineType": "1001",
			"description": "早，准备开始新的冒险了么？",
			"voicefile": "VO_friendship/VO_venti/vo_venti_dialog_greetingMorning"
		},
		{
			"voicelineId": 11109,
			"title": "中午好…",
			"voicelineType": "1002",
			"description": "咕…肚子饿了，又不敢再去晨曦酒庄蹭饭…啊，是你呀！欸嘿，要出门么？带我一个呗。",
			"voicefile": "VO_friendship/VO_venti/vo_venti_dialog_greetingNoon"
		},
		{
			"voicelineId": 11110,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "我还不困哦。要我陪你走走么？",
			"voicefile": "VO_friendship/VO_venti/vo_venti_dialog_greetingNight"
		},
		{
			"voicelineId": 11111,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "欸？要休息了吗？嘿嘿，晚安。",
			"voicefile": "VO_friendship/VO_venti/vo_venti_dialog_greetingNight2"
		},
		{
			"voicelineId": 11112,
			"title": "关于温迪自己…",
			"voicelineType": "2001",
			"description": "练习？哼哼，我当然不需要啦，在这提瓦特就没有我不会唱的歌。",
			"voicefile": "VO_friendship/VO_venti/vo_venti_dialog_idle"
		},
		{
			"voicelineId": 11113,
			"title": "关于我们·新歌",
			"voicelineType": "2002",
			"description": "我想把你写进新歌里！欸，怎么露出这种表情呀，担心付不起稿费吗？哈哈，摩拉就不要了，不如…跟我再讲讲你的故事吧！",
			"voicefile": "VO_friendship/VO_venti/vo_venti_dialog_close1"
		},
		{
			"voicelineId": 11114,
			"title": "关于我们·勇者",
			"voicelineType": "2003",
			"description": "唔…嗯！嘿嘿，等到勇者救出了{MATEAVATAR#SEXPRO[INFO_MALE_PRONOUN_BOYD|INFO_FEMALE_PRONOUN_GIRLD]}，我会第一时间让全大陆传唱这首歌谣。",
			"voicefile": "VO_friendship/VO_venti/vo_venti_dialog_close2_heroine",
			"voicefile_male": "VO_friendship/VO_venti/vo_venti_dialog_close2_hero",
			"hasGenderedVoicefile": true,
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 11115,
			"title": "关于我们·使徒",
			"voicelineType": "2004",
			"description": "夜风凉爽，是适合成为神的使徒的天气！那么事不宜迟，快来向我献上供奉吧？",
			"voicefile": "VO_friendship/VO_venti/vo_venti_dialog_close3",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				},
				{
					"unlockText": "完成「巨龙与自由之歌」剧情",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						39604
					]
				}
			]
		},
		{
			"voicelineId": 11116,
			"title": "关于我们·请求",
			"voicelineType": "2005",
			"description": "欸，要风神我更加努力一点？那你是不是也要展示点诚意呢。比如虔诚、热爱，或者说…",
			"voicefile": "VO_friendship/VO_venti/vo_venti_dialog_close4",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
