{
	"id": 10000006,
	"name": "Lisa",
	"friendLines": [
		{
			"voicelineId": 5100,
			"title": "初次见面…",
			"voicelineType": "1000",
			"description": "嗨，小可爱，你是新来的助理吗？啊？魔导师大人？那是很久以前的事啦，现在的我不过是一介图书管理员而已。放心吧，姐姐会照顾好你的。那么…",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_draw_appear"
		},
		{
			"voicelineId": 5101,
			"title": "闲聊·下午茶",
			"voicelineType": "1101",
			"description": "明明是该喝下午茶的时间。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_explore_idle_01"
		},
		{
			"voicelineId": 5102,
			"title": "闲聊·任务",
			"voicelineType": "1102",
			"description": "任务是做不完的，还不如陪姐姐…聊会儿天。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_explore_idle_02"
		},
		{
			"voicelineId": 5103,
			"title": "闲聊·不宜出门",
			"voicelineType": "1103",
			"description": "今天不宜出门…欸，我昨天也是这么说的？",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_explore_idle_03"
		},
		{
			"voicelineId": 5104,
			"title": "下雨的时候…",
			"voicelineType": "1201",
			"description": "这种天气，泡一杯茶，坐着看雨多好。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_weather_heavyRain_01"
		},
		{
			"voicelineId": 5105,
			"title": "打雷的时候…",
			"voicelineType": "1202",
			"description": "在雷雨天训练事半功倍哦~呵呵，骗你的。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_weather_thunder_01"
		},
		{
			"voicelineId": 5106,
			"title": "起风的时候…",
			"voicelineType": "1203",
			"description": "天气真好，能躺下来补个觉就更好了。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_weather_breezy_01"
		},
		{
			"voicelineId": 5107,
			"title": "早上好…",
			"voicelineType": "1001",
			"description": "啊…早安，琴。嗯？不好意思，我看错了。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_greetingMorning"
		},
		{
			"voicelineId": 5108,
			"title": "中午好…",
			"voicelineType": "1002",
			"description": "午休时间到了…",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_greetingNoon"
		},
		{
			"voicelineId": 5109,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "你还不睡吗？我已经困了呢…",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_greetingNight"
		},
		{
			"voicelineId": 5110,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "你努力的样子，姐姐全都见到了哦。嗯…让我想想，给你什么奖励吧…",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_greetingNight2"
		},
		{
			"voicelineId": 5111,
			"title": "关于丽莎自己…",
			"voicelineType": "2001",
			"description": "这可不是偷懒呀。谁不想用最省力的方法达到目的呢？",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_idle"
		},
		{
			"voicelineId": 5112,
			"title": "关于我们·关系",
			"voicelineType": "2002",
			"description": "你想把我当作你的老师、前辈，还是朋友？都可以哦。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_close1"
		},
		{
			"voicelineId": 5113,
			"title": "关于我们·本领",
			"voicelineType": "2003",
			"description": "哎呀，再不拿出点真本事出来，就对不起你的努力了吧。嗯哼…也好也好。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_close2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 5114,
			"title": "关于我们·仪式",
			"voicelineType": "2004",
			"description": "再等一下，「将悔罪的记号吻在弟子的前额」…好了，那么仪式就完成了~今后你就是我丽莎的首席弟子了哟。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_close3",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 5115,
			"title": "关于「神之眼」…",
			"voicelineType": "3001",
			"description": "「神之眼」啊…那可是导引元素力所必要的装置呢。唯独在你身上却是例外。唔…究竟是什么原理呢，啊，有了，你…有空陪姐姐做个实验么？",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5116,
			"title": "有什么想要分享…",
			"voicelineType": "3002",
			"description": "图书馆楼上的炼金工坊，可以随意使用哦？不过进去之前记得敲门，如果撞见什么不该见到的东西那就麻烦了。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_share"
		},
		{
			"voicelineId": 5117,
			"title": "感兴趣的见闻…",
			"voicelineType": "3003",
			"description": "嘟嘟莲是有着解毒功效的药草。有了它，就不用再担心魔药的副作用了…哼哼。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_pref_gift"
		},
		{
			"voicelineId": 5118,
			"title": "关于琴…",
			"voicelineType": "4001",
			"description": "琴是个称职的好团长。看到她认真工作的样子，就连我也忍不住想要多帮她一把。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_qin_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5119,
			"title": "关于安柏…",
			"voicelineType": "4002",
			"description": "小安柏刚进骑士团的样子我还记得呢，一转眼都那么能干了。啊，再过几年，又会变成什么样呢？",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_ambor_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5120,
			"title": "关于芭芭拉…",
			"voicelineType": "4003",
			"description": "芭芭拉的歌，你听过么？哼哼，听说她的旋律里有种治愈的魔力在流动喔。和我的魔药相比，不知道哪边更合你的胃口呢？",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_barbara_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5121,
			"title": "关于迪卢克…",
			"voicelineType": "4004",
			"description": "哦，想听迪卢克的故事？哈哈，那我知道一个更好的聊天对象。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_diluc_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5122,
			"title": "关于雷泽…",
			"voicelineType": "4005",
			"description": "小狼狼，来来，过来这边。今天就临时来一场实战练习吧，对手我已经给你找好了哟。欸？怎么跑了，真是的…",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_razor_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5123,
			"title": "关于可莉…",
			"voicelineType": "4006",
			"description": "上次小可莉想借用我的工坊做实验，被我拒绝了，她可是生了我一天的气呢。嗯？你问第二天？第二天，她当然又高高兴兴地出门去玩了。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_klee_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5124,
			"title": "关于莫娜…",
			"voicelineType": "4007",
			"description": "莫娜？似乎是一位很优秀的魔导师小姐呢。但我并不是她们「魔女会」的成员哦。古树考察，组织茶会，一听就是麻烦的事情吧。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_mona_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5125,
			"title": "关于阿贝多…",
			"voicelineType": "4008",
			"description": "姐姐我觉得，阿贝多是很优秀的炼金术士。跟他只需要用三言两语，就能得到不错的结论。而且脸也长得好看。和小可爱你很像喔，嗯。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_albedo_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5126,
			"title": "关于菲谢尔…",
			"voicelineType": "4009",
			"description": "菲谢尔…哦，那个总是来图书馆借幻想小说的女孩子。每次还书都会提前三天，对书籍也十分爱护，是个好孩子呢。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_fischl_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5127,
			"title": "关于诺艾尔…",
			"voicelineType": "4010",
			"description": "拜托诺艾尔清扫书架的时候，总感觉这孩子，越是疲惫就越开心，嗯…到底是怎么回事呢？",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_noel_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5128,
			"title": "关于砂糖…",
			"voicelineType": "4011",
			"description": "没事就不要去打扰小砂糖哦，会吓到她的。呵呵，她擅长和各种各样的生物打交道，却唯独，不怎么善于和人类相处呢。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_sucrose_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5171,
			"title": "关于赛诺…",
			"voicelineType": "4012",
			"description": "离开须弥之后，除了和恩师保持礼节性的通信，我和教令院已经没有什么往来了。幸好，不是每位被寄予了重望的弟子，都像我一样怠惰…听说那位师弟赛诺就相当活跃呢。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_cyno_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5172,
			"title": "关于柯莱…",
			"voicelineType": "4013",
			"description": "小柯莱每次写信都会使用新词，尝试与之前不同的表述方式。呵呵，也难怪她进步得那么快。除了自己的努力之外，她那位善良而又热忱的师父，一定也给予了很多帮助吧。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_collei_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5173,
			"title": "关于米卡…",
			"voicelineType": "4014",
			"description": "真羡慕霍夫曼先生有这样一位擅长照顾人的弟弟。不仅帮着打点生活起居，连喝醉后的护理也无微不至。啊，我也好想什么都不用操心，懒洋洋地度过每一天啊…嗯？你说我现在也差不多？哼，小可爱…胆子变大了嘛。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_teammate_mika_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5129,
			"title": "想要了解丽莎·其一",
			"voicelineType": "5001",
			"description": "那么，今天想学习哪种元素力的知识呢？",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_friendship_01"
		},
		{
			"voicelineId": 5130,
			"title": "想要了解丽莎·其二",
			"voicelineType": "5002",
			"description": "我的过去？嗯？想要更多地了解我一些吗？…可今天我恰巧有点累了。不过这份心意，姐姐我还是很高兴的。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到3后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 5131,
			"title": "想要了解丽莎·其三",
			"voicelineType": "5003",
			"description": "我教雷泽掌握元素力量的时候，完全没想到那个孩子的悟性有那么好。仔细想想的话，你也相当能干啊，小可爱。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 5132,
			"title": "想要了解丽莎·其四",
			"voicelineType": "5004",
			"description": "什么？想学习须弥教令院的魔法？可对现在的你来说，负担会有些重…啊？至少想学会入门的知识？那先看完这本《绪论》吧，也就800多页而已。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到5后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				}
			]
		},
		{
			"voicelineId": 5133,
			"title": "想要了解丽莎·其五",
			"voicelineType": "5005",
			"description": "总有一天，这「眼」中的力量会毁灭一切的吧…到了那时，如果有你在的话…啊，没什么。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_friendship_05",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 5134,
			"title": "丽莎的爱好…",
			"voicelineType": "6001",
			"description": "小可爱，你想要来挑一瓶神奇的药水么？喝下去之前完全不知道会有什么效果，呼呼。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_pref_hobby"
		},
		{
			"voicelineId": 5135,
			"title": "丽莎的烦恼…",
			"voicelineType": "6002",
			"description": "二楼的审讯大厅已经闲置两年了，明明给我用作元素工坊的话，就不至于那么荒废…赫塔小姐真是死脑筋。",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_dialog_annoyed"
		},
		{
			"voicelineId": 5136,
			"title": "喜欢的食物…",
			"voicelineType": "6003",
			"description": "我喜欢蔬菜汤哦。只要把所有的蔬菜全都丢进一口大锅子里，再用长柄的勺子搅一搅…欸，问我好不好吃？哼哼，来，啊——",
			"voicefile": "VO_friendship/VO_lisa/vo_lisa_pref_foodLike"
		},
		{
			"voicelineId": 5137,
			"title": "讨厌的食物…",
			"voicelineType": "6004",
