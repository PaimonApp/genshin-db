	"name": "Qiqi",
	"friendLines": [
		{
			"voicelineId": 24009,
			"title": "初次见面…",
			"voicelineType": "1000",
			"description": "我是七七，是个僵尸…啊，还要说什么来着。",
			"voicefile": "VO_friendship/VO_qiqi/vo_qiqi_draw_appear"
		},
		{
			"voicelineId": 24010,
			"title": "闲聊·自言自语",
			"voicelineType": "1101",
			"description": "咦，刚刚…我想说什么来着…",
			"voicefile": "VO_friendship/VO_qiqi/vo_qiqi_explore_idle_01"
		},
		{
			"voicelineId": 24011,
			"title": "闲聊·怕热",
			"voicelineType": "1102",
			"description": "想去凉快点的地方。",
			"voicefile": "VO_friendship/VO_qiqi/vo_qiqi_explore_idle_02"
		},
		{
			"voicelineId": 24012,
			"title": "闲聊·锻炼",
			"voicelineType": "1103",
			"description": "七二三四，七二三四…五六七七，五六七七…",
			"voicefile": "VO_friendship/VO_qiqi/vo_qiqi_explore_idle_03"
		},
		{
			"voicelineId": 24013,
			"title": "下雨的时候…",
			"voicelineType": "1201",
			"description": "又忘记带伞了。",
			"voicefile": "VO_friendship/VO_qiqi/vo_qiqi_weather_heavyRain_01"
		},
		{
			"voicelineId": 24014,
			"title": "下雪的时候…",
			"voicelineType": "1202",
			"description": "想堆雪人…可以陪我吗？",
			"voicefile": "VO_friendship/VO_qiqi/vo_qiqi_weather_snowy_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
