{
	"id": 10000020,
	"name": "Razor",
	"friendLines": [
		{
			"voicelineId": 9100,
			"title": "初次见面…",
			"voicelineType": "1000",
			"description": "你的气味…好闻。一起狩猎吧！",
			"voicefile": "VO_friendship/VO_razor/vo_razor_draw_appear"
		},
		{
			"voicelineId": 9101,
			"title": "闲聊·气味",
			"voicelineType": "1101",
			"description": "唔，兔子的味道…",
			"voicefile": "VO_friendship/VO_razor/vo_razor_explore_idle_01"
		},
		{
			"voicelineId": 9102,
			"title": "闲聊·饿",
			"voicelineType": "1102",
			"description": "走吗，肚子饿了。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_explore_idle_02"
		},
		{
			"voicelineId": 9103,
			"title": "闲聊·望风",
			"voicelineType": "1103",
			"description": "我去望风，走了叫我。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_explore_idle_03"
		},
		{
			"voicelineId": 9104,
			"title": "打雷的时候·提醒",
			"voicelineType": "1201",
			"description": "怕就快点躲起来。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_weather_thunder_01"
		},
		{
			"voicelineId": 9105,
			"title": "打雷的时候·带路",
			"voicelineType": "1204",
			"description": "避开树，跟我来！",
			"voicefile": "VO_friendship/VO_razor/vo_razor_weather_thunder_02"
		},
		{
			"voicelineId": 9106,
			"title": "起风的时候…",
			"voicelineType": "1202",
			"description": "风…唔…舒服。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_weather_breezy_01"
		},
		{
			"voicelineId": 9107,
			"title": "刮大风了…",
			"voicelineType": "1203",
			"description": "风…嗯…扎扎的，有点疼。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_weather_gale_01"
		},
		{
			"voicelineId": 9108,
			"title": "早上好…",
			"voicelineType": "1001",
			"description": "太阳出来了。狩猎一起去？",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_greetingMorning"
		},
		{
			"voicelineId": 9109,
			"title": "中午好…",
			"voicelineType": "1002",
			"description": "大块吃肉，开心！",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_greetingNoon"
		},
		{
			"voicelineId": 9110,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "你去睡觉吧，我看月亮。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_greetingNight"
		},
		{
			"voicelineId": 9111,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "你睡觉，我看守。明天一起狩猎。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_greetingNight2"
		},
		{
			"voicelineId": 9112,
			"title": "关于雷泽自己…",
			"voicelineType": "2001",
			"description": "我是狼。人的爸爸、妈妈，没有。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_idle"
		},
		{
			"voicelineId": 9113,
			"title": "关于我们·朋友",
			"voicelineType": "2002",
			"description": "你是朋友。我和你一起狩猎。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_close1"
		},
		{
			"voicelineId": 9114,
			"title": "关于我们·武器",
			"voicelineType": "2003",
			"description": "我有铁的爪子和雷的牙。我保护你。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_close2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 9115,
			"title": "关于「神之眼」…",
			"voicelineType": "3001",
			"description": "「神之眼」，师父帮忙。雷的牙打架，保护「卢皮卡」。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9116,
			"title": "有什么想要分享…",
			"voicelineType": "3002",
			"description": "晚上，星星很好看。天空，湖里，都有很多很多星星。\n晚上，城市里，也有很多星星。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_share"
		},
		{
			"voicelineId": 9117,
			"title": "感兴趣的见闻…",
			"voicelineType": "3003",
			"description": "我的「卢皮卡」的狼。它们是家人。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_pref_gift"
		},
		{
			"voicelineId": 9118,
			"title": "关于丽莎…",
			"voicelineType": "4001",
			"description": "师父的雷，天空会变黑。很可怕，很强。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_lisa_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「卢皮卡的意义」剧情",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						45406
					]
				}
			]
		},
		{
			"voicelineId": 9119,
			"title": "关于芭芭拉…",
			"voicelineType": "4002",
			"description": "芭芭拉，亮晶晶的女孩子。很善良。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_barbara_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9120,
			"title": "关于迪卢克…",
			"voicelineType": "4003",
			"description": "迪卢克，红红的、黑黑的。又冷又热。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_diluc_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9121,
			"title": "关于温迪…",
			"voicelineType": "4004",
			"description": "诗歌…唔，不明白。虽然不明白，「卢皮卡」喜欢。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_venti_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9122,
			"title": "关于班尼特…",
			"voicelineType": "4005",
			"description": "班尼特，话很多，和他一起，吃不饱。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_bennett_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9123,
			"title": "关于可莉…",
			"voicelineType": "4006",
			"description": "可莉，朋友！朋友，但是，头疼。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_klee_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「卢皮卡的意义」剧情",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						45406
					]
				}
			]
		},
		{
			"voicelineId": 9124,
			"title": "关于法尔伽·陌生",
			"voicelineType": "4008",
			"description": "法尔伽？不认识…",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_varka_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9125,
			"title": "关于法尔伽·回忆",
			"voicelineType": "4009",
			"description": "哦…法尔伽，是名字。那个男人，高大。雷泽名字，铁的爪子，他给的。唔…想见他。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_varka_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 9126,
			"title": "想要了解雷泽·其一",
			"voicelineType": "5001",
			"description": "你的气味，善良的味道。喜欢吃肉？",
			"voicefile": "VO_friendship/VO_razor/vo_razor_friendship_01"
		},
		{
			"voicelineId": 9127,
			"title": "想要了解雷泽·其二",
			"voicelineType": "5002",
			"description": "你很强。和狼一样强。喜欢和你狩猎。收获很多。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到3后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 9128,
			"title": "想要了解雷泽·其三",
			"voicelineType": "5003",
			"description": "卢皮卡是「狼的家」。不对，是「狼的家人」。我的卢皮卡在树林里。它们保护我，说我是卢皮卡。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到4后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9129,
			"title": "想要了解雷泽·其四",
			"voicelineType": "5004",
			"description": "我只有卢皮卡。\n但是，就算是卢皮卡，我没有四只脚，不是真的狼。有时候心里难过。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到5后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				}
			]
		},
		{
			"voicelineId": 9130,
			"title": "想要了解雷泽·其五",
			"voicelineType": "5005",
			"description": "和你一起奔跑开心，和你一起狩猎开心。和你一起，我是狼，我是人，都没关系。\n你是朋友，你是我的「卢皮卡」。我想要你也开心。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_friendship_05",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等级达到6后开启",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 9131,
			"title": "雷泽的爱好…",
			"voicelineType": "6001",
			"description": "喜欢卢皮卡。喜欢树林。喜欢朋友。喜欢奔跑。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_pref_hobby"
		},
		{
			"voicelineId": 9132,
			"title": "雷泽的烦恼…",
			"voicelineType": "6002",
			"description": "说话很累，很难。但是，不说话不行。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_annoyed"
		},
		{
			"voicelineId": 9133,
			"title": "喜欢的食物…",
			"voicelineType": "6003",
			"description": "喜欢肉。喜欢土豆。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_pref_foodLike"
		},
		{
			"voicelineId": 9134,
			"title": "讨厌的食物…",
			"voicelineType": "6004",
			"description": "蔬菜不好。我吃的肉才吃菜。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_pref_foodDislike"
		},
		{
			"voicelineId": 9163,
			"title": "收到赠礼·其一",
			"voicelineType": "6007",
			"description": "气味很香，味道一样好。喜欢。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_spice_pref_01"
		},
		{
			"voicelineId": 9164,
			"title": "收到赠礼·其二",
			"voicelineType": "6008",
			"description": "吃了有力气，很开心。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_spice_normal_01"
		},
		{
			"voicelineId": 9165,
			"title": "收到赠礼·其三",
			"voicelineType": "6009",
			"description": "吃，不好。不吃，不好。有些难过。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_spice_dislike_01"
		},
		{
			"voicelineId": 9135,
			"title": "生日…",
			"voicelineType": "7001",
			"description": "今天是你的…你是在很多很多个月以前的，今天出生的。想和你一起开心。今天一起吃肉。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_pref_birthday",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"conditionType": "FETTER_COND_PLAYER_BIRTHDAY",
					"paramList": []
				}
			]
		},
		{
			"voicelineId": 9136,
			"title": "突破的感受·起",
			"voicelineType": "8001",
			"description": "手臂，更粗了…？",
			"voicefile": "VO_friendship/VO_razor/vo_razor_starUp_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等级「1」阶后开启",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						1
					]
				}
			]
		},
		{
			"voicelineId": 9137,
			"title": "突破的感受·承",
			"voicelineType": "8002",
			"description": "磨爪子，磨牙，要更强！",
			"voicefile": "VO_friendship/VO_razor/vo_razor_starUp_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等级「2」阶后开启",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						2
					]
				}
			]
		},
		{
			"voicelineId": 9138,
			"title": "突破的感受·转",
			"voicelineType": "8003",
			"description": "跑，狩猎。不够，不行！我要…更强。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_starUp_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等级「4」阶后开启",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9139,
			"title": "突破的感受·合",
			"voicelineType": "8004",
			"description": "牙齿，爪子，腿。更强，更有力气…保护你！",
			"voicefile": "VO_friendship/VO_razor/vo_razor_starUp_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等级「6」阶后开启",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						6
					]
				}
			]
		}
	],
