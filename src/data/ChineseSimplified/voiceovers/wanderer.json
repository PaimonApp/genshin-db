		},
		{
			"voicelineId": 75077,
			"title": "重受击·其二",
			"voicelineType": "910002",
			"description": "…可笑。",
			"voicefile": "VO_gameplay/VO_wanderer/vo_wanderer_battle_hit_H_03"
		},
		{
			"voicelineId": 75078,
			"title": "重受击·其三",
			"voicelineType": "910003",
			"description": "…你死定了。",
			"voicefile": "VO_gameplay/VO_wanderer/vo_wanderer_battle_hit_H_06"
		},
		{
			"voicelineId": 75079,
			"title": "重受击·其四",
			"voicelineType": "910004",
			"description": "等着求饶吧。",
			"voicefile": "VO_gameplay/VO_wanderer/vo_wanderer_battle_hit_H_07"
		},
		{
			"voicelineId": 75080,
			"title": "重受击·其五",
			"voicelineType": "910005",
			"description": "做好付出代价的准备。",
