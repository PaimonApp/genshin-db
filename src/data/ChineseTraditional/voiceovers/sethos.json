{
	"id": 10000097,
	"name": "Sethos",
	"friendLines": [
		{
			"voicelineId": 97000,
			"title": "初次見面…",
			"voicelineType": "1006",
			"description": "這位{M#先生}{F#小姐}，是在叫我嗎？{F#妳}{M#你}好{F#妳}{M#你}好，我叫賽索斯。{F#妳}{M#你}呢，找我有什麼事嗎？哎呀，也不要光站著了，我知道一個適合坐下來聊天的好地方，跟我來吧。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_draw_appear_01b",
			"voicefile_male": "VO_friendship/VO_sethos/vo_sethos_draw_appear_01a",
			"hasGenderedVoicefile": true
		},
		{
			"voicelineId": 97001,
			"title": "閒聊·思考",
			"voicelineType": "1101",
			"description": "過度思考「為什麼」很容易鑽牛角尖，不如放過自己。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_character_idle_01"
		},
		{
			"voicelineId": 97002,
			"title": "閒聊·動靜",
			"voicelineType": "1102",
			"description": "{F#妳}{M#你}聽到了嗎？那邊有動靜。先下手為強，我想去湊湊熱鬧。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_character_idle_02"
		},
		{
			"voicelineId": 97003,
			"title": "閒聊·上學",
			"voicelineType": "1103",
			"description": "教令院裡面怪人真多，搞得我都想考進去上個學了。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_character_idle_03"
		},
		{
			"voicelineId": 97004,
			"title": "下雨的時候…",
			"voicelineType": "1201",
			"description": "沙漠裡極少下雨，所以就讓我享受一下這樣的天氣吧，不用幫我撐傘啦。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_weather_heavyRain_01"
		},
		{
			"voicelineId": 97005,
			"title": "打雷的時候…",
			"voicelineType": "1202",
			"description": "糟了糟了糟了，快去地勢低的地方…",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_weather_thunder_01"
		},
		{
			"voicelineId": 97006,
			"title": "下雪的時候…",
			"voicelineType": "1203",
			"description": "嘶…快快快，跑起來，跑起來就不冷了！",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_weather_snowy_01"
		},
		{
			"voicelineId": 97007,
			"title": "陽光很好…",
			"voicelineType": "1204",
			"description": "天氣真好，不找個地方露營太浪費了。走嗎？一起去綠洲釣魚？",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_weather_sunny_01"
		},
		{
			"voicelineId": 97008,
			"title": "在沙漠的時候…",
			"voicelineType": "1205",
			"description": "{F#妳}{M#你}有在沙地上畫畫過嗎？從高處看會很壯觀哦！就是要找個風小的地方。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_weather_desert_01"
		},
		{
			"voicelineId": 97009,
			"title": "早安…",
			"voicelineType": "1001",
			"description": "早早早，要跟我去吃點東西嗎？我從昨晚就惦記著要做幾個烤肉卷了。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_greetingMorning"
		},
		{
			"voicelineId": 97010,
			"title": "午安…",
			"voicelineType": "1002",
			"description": "現在喝咖啡的話就沒辦法享受午覺了哦。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_greetingNoon"
		},
		{
			"voicelineId": 97011,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "雖然沙漠白天沒什麼生機，但晚上總有很好看的星星。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_greetingNight"
		},
		{
			"voicelineId": 97012,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "睡了睡了，準備迎接明天的太陽！",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_greetingNight2"
		},
		{
			"voicelineId": 97013,
			"title": "關於賽索斯自己·印象",
			"voicelineType": "2001",
			"description": "有些長輩說我像野生動物一樣難以控制。不是要說他們的印象有錯，但首先，想要控制別人的想法有點不太對，{F#妳}{M#你}說是吧？",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_idle"
		},
		{
			"voicelineId": 97014,
			"title": "關於賽索斯自己·首領",
			"voicelineType": "2005",
			"description": "老實說，做首領還是滿有壓力的，更不用說是在緘默之殿。不過受爺爺耳濡目染了這麼多年，我清楚那些最不能讓步的底線。而且經歷了之前的決鬥之儀，又認識了那麼多新朋友，我也明白緘默之殿之後要走的路在哪裡了。放心吧，不會搞砸的。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_idle2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「守諾者」劇情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						13039
					]
				}
			]
		},
		{
			"voicelineId": 97015,
			"title": "關於我們·閒聊",
			"voicelineType": "2003",
			"description": "抱著想要和對方打好關係的目的刻意談話，很容易陷入僵局。就像這樣一起走走，隨便聊聊，反而更容易熟悉起來。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_close"
		},
		{
			"voicelineId": 97016,
			"title": "關於我們·篝火",
			"voicelineType": "2002",
			"description": "夜晚的篝火，既能為人提供溫度，又能防範野獸。{F#妳}{M#你}給我的感覺就像篝火一樣。\n說得太抽象了？唉呀，就是把{F#妳}{M#你}當朋友的意思啦。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_close1",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到6後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				},
				{
					"unlockText": "完成「守諾者」劇情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						13039
					]
				}
			]
		},
		{
			"voicelineId": 97017,
			"title": "關於「神之眼」…",
			"voicelineType": "3006",
			"description": "沙漠人獲得神之眼時，大夥都會說這是誰誰誰的恩賜，實際的人選視各自信仰而定。看一群人為了別人的神之眼爭論赫曼努比斯大人和索貝克大人究竟誰更強，怪好玩的。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_pendant_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 97018,
			"title": "有什麼想要分享…",
			"voicelineType": "3002",
			"description": "如果去沙漠旅行，夜間趕路是個不錯的選擇。氣溫的確寒冷，但可以避免在炎熱的光照下流失過多水分。多帶件衣服總比四處尋找水源方便多了。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_share_01"
		},
		{
			"voicelineId": 97019,
			"title": "感興趣的見聞…",
			"voicelineType": "3003",
			"description": "「悼靈花」這個名字不知道是誰取的，還滿浪漫的。要是我的話，也許只會以為它是被太陽曬蔫了才垂頭喪氣的。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_pref_gift_01"
		},
		{
			"voicelineId": 97020,
			"title": "關於賽諾…",
			"voicelineType": "4001",
			"description": "賽諾很優秀，無愧於赫曼努比斯力量的傳承。作為「大風紀官」，他對知識與智慧有著至高的敬畏之心。如果教令院能與緘默之殿再次合作，有他在我會放心很多。\n哦對了，他還有個難得的優點，就是講話很好笑。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_teammate_cyno_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「守諾者」劇情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						13039
					]
				}
			]
		},
		{
			"voicelineId": 97021,
			"title": "關於提納里…",
			"voicelineType": "4002",
			"description": "提納里滿厲害的。我後來才聽說他不太適應沙漠的氣候，決鬥之儀那時候完全沒看出來。我也沒想到他會寫信邀請我去化城郭做客，說是讓我有機會見見賽諾的朋友們。{F#妳}{M#你}覺得他們平常吃飯都會講那種笑話嗎？真期待！",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_teammate_tighnari_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「守諾者」劇情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						13039
					]
				}
			]
		},
		{
			"voicelineId": 97022,
			"title": "關於艾爾海森和卡維…",
			"voicelineType": "4003",
			"description": "我在酒館見過他們，也在咖啡館見過…好像在大巴扎也有一次。那位建築師總是嘀嘀咕咕一些不想被別人知道的事情，但實際上嘛…欸，不是我故意偷聽，是那兩位先生湊在一起很難不引人注意吧？",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_teammate_alhaitham&kaveh_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 97023,
			"title": "關於流浪者…",
			"voicelineType": "4004",
			"description": "這位小兄弟，是不是外國來的留學生啊？帽子滿別緻的，有次我想湊近仔細看看，但一眨眼的功夫他就咻地一下消失了。可惜，下次不會再讓他跑了。而且聽人家說那位兄弟很有想法，我要和他多聊幾下！",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_teammate_wanderer_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 97024,
			"title": "關於迪希雅…",
			"voicelineType": "4005",
			"description": "「熾鬃之獅」——沙漠裡人人都聽說過這個名號。{F#妳}{M#你}也知道，鍍金旅團裡不全都是好人，像迪希雅這樣正直可靠的就更少了。如果{F#妳}{M#你}要來大赤沙海探險，迪希雅是做嚮導第二好的人選。第一嘛…當然是我囉？",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_teammate_dehya_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 97025,
			"title": "關於坎蒂絲…",
			"voicelineType": "4006",
			"description": "{F#妳}{M#你}被坎蒂絲招待過嗎？她做的飯超大份的，連我都差點沒吃完。不過吃飯就好了，千萬別吃她的拳頭。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_teammate_candace_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 97026,
			"title": "關於琺露珊…",
			"voicelineType": "4007",
			"description": "很少見到比我還自來熟的人！在教令院附近閒逛時被她當成新生，非要帶我進去參觀，還說什麼現在加入知論派就能享受二十種語言教材大禮包，包準教到會，嚇我一大跳。問了別的學生才知道這是他們院的內部笑話，根本沒有這種畢業要求。所以到底哪裡好笑？只會讓人更不想加入吧？",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_teammate_faruzan_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 97027,
			"title": "關於妮露…",
			"voicelineType": "4008",
			"description": "我是個閒不下來的人，但不介意在祖拜爾劇場多站一會，看看這位小姐的舞蹈。能夠讓人內心寧靜也是一種很難得的能力。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_teammate_nilou_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 97028,
			"title": "想要瞭解賽索斯·其一",
			"voicelineType": "5001",
			"description": "想打聽事情？{F#妳}{M#你}找對人了。萬一有我不知道的東西，也可以幫{F#妳}{M#你}四處問問。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_friendship_01"
		},
		{
			"voicelineId": 97029,
			"title": "想要瞭解賽索斯·其二",
			"voicelineType": "5002",
			"description": "{F#妳}{M#你}要是有什麼裝備或工具壞掉可以叫我來修。{F#妳}{M#你}問我是做什麼工作的？哎呀，當然是無業遊民了，不然哪有這麼多閒工夫做手工呢？",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到3後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 97030,
			"title": "想要瞭解賽索斯·其三",
			"voicelineType": "5003",
			"description": "我喜歡徒步旅行。提前規劃好路線、整理行囊，挑選好日子踏上旅途，這種有條不紊的感覺很不錯。當然，我不會把行程排得很滿，也要享受突如其來的邂逅嘛。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 97031,
			"title": "想要瞭解賽索斯·其四",
			"voicelineType": "5004",
			"description": "在大巴扎閒逛很好玩的喔。我會挑選各種香料混在一起，搭配現烤的肉類食用。我不會特意去記配方，所以很多時候都是「單次限定食譜」。怎麼樣，有沒有興趣吃吃看這種限時限量、獨一無二的美味？",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到5後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				}
			]
		},
		{
			"voicelineId": 97032,
			"title": "想要瞭解賽索斯·其五",
			"voicelineType": "5005",
			"description": "自懂事以來我就在緘默之殿生活，老實說沒有太多的選擇。能夠承受赫曼努比斯的力量對個人而言不知是福多還是禍多，但的確為我的生活賦予了更崇高的意義。\n人生中的遺憾有千千萬個，可以圓滿解決一件事我就很滿足了。現在我成為了緘默之殿的新首領，赫曼努比斯之力也有了適合的歸屬，爺爺應該也很欣慰吧…",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_friendship_05",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到6後開啟",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				},
				{
					"unlockText": "完成「守諾者」劇情",
					"conditionType": "FETTER_COND_FINISH_PARENT_QUEST",
					"paramList": [
						13039
					]
				}
			]
		},
		{
			"voicelineId": 97033,
			"title": "賽索斯的愛好…",
			"voicelineType": "6001",
			"description": "老是待在屋子裡滿無聊的，無論天氣好壞，我都喜歡出門走走。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_pref_hobby"
		},
		{
			"voicelineId": 97034,
			"title": "賽索斯的煩惱…",
			"voicelineType": "6012",
			"description": "出遠門前一定要做好準備，尤其是前往沒去過的地方。當初第一次在樹林裡過夜不知道要帶防潮墊，把我害慘了。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_dialog_annoyed_01"
		},
		{
			"voicelineId": 97035,
			"title": "喜歡的食物…",
			"voicelineType": "6003",
			"description": "我喜歡調配香料，是為了能夠讓肉變得更好吃，不要本末倒置了哦。",
			"voicefile": "VO_friendship/VO_sethos/vo_sethos_pref_foodLike"
