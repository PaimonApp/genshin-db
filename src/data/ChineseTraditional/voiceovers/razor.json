{
	"id": 10000020,
	"name": "Razor",
	"friendLines": [
		{
			"voicelineId": 9100,
			"title": "初次見面…",
			"voicelineType": "1000",
			"description": "{F#妳}{M#你}的氣味…好聞。一起狩獵吧！",
			"voicefile": "VO_friendship/VO_razor/vo_razor_draw_appear"
		},
		{
			"voicelineId": 9101,
			"title": "閒聊·氣味",
			"voicelineType": "1101",
			"description": "唔，兔子的味道…",
			"voicefile": "VO_friendship/VO_razor/vo_razor_explore_idle_01"
		},
		{
			"voicelineId": 9102,
			"title": "閒聊·餓",
			"voicelineType": "1102",
			"description": "走嗎，肚子餓了。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_explore_idle_02"
		},
		{
			"voicelineId": 9103,
			"title": "閒聊·把風",
			"voicelineType": "1103",
			"description": "我去望風，走了叫我。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_explore_idle_03"
		},
		{
			"voicelineId": 9104,
			"title": "打雷的時候·提醒",
			"voicelineType": "1201",
			"description": "怕就快點躲起來。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_weather_thunder_01"
		},
		{
			"voicelineId": 9105,
			"title": "打雷的時候·帶路",
			"voicelineType": "1204",
			"description": "避開樹，跟我來！",
			"voicefile": "VO_friendship/VO_razor/vo_razor_weather_thunder_02"
		},
		{
			"voicelineId": 9106,
			"title": "起風的時候…",
			"voicelineType": "1202",
			"description": "風…唔…舒服。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_weather_breezy_01"
		},
		{
			"voicelineId": 9107,
			"title": "颳大風了…",
			"voicelineType": "1203",
			"description": "風…嗯…紮紮的，有點疼。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_weather_gale_01"
		},
		{
			"voicelineId": 9108,
			"title": "早安…",
			"voicelineType": "1001",
			"description": "太陽出來了。狩獵一起去？",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_greetingMorning"
		},
		{
			"voicelineId": 9109,
			"title": "午安…",
			"voicelineType": "1002",
			"description": "大塊吃肉，開心！",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_greetingNoon"
		},
		{
			"voicelineId": 9110,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "{F#妳}{M#你}去睡覺吧，我看月亮。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_greetingNight"
		},
		{
			"voicelineId": 9111,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "{F#妳}{M#你}睡覺，我看守。明天一起狩獵。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_greetingNight2"
		},
		{
			"voicelineId": 9112,
			"title": "關於雷澤自己…",
			"voicelineType": "2001",
			"description": "我是狼。人的爸爸、媽媽，沒有。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_idle"
		},
		{
			"voicelineId": 9113,
			"title": "關於我們·朋友",
			"voicelineType": "2002",
			"description": "{F#妳}{M#你}是朋友。我和{F#妳}{M#你}一起狩獵。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_close1"
		},
		{
			"voicelineId": 9114,
			"title": "關於我們·武器",
			"voicelineType": "2003",
			"description": "我有鐵的爪子和雷的牙。我保護{F#妳}{M#你}。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_close2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到6後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 9115,
			"title": "關於「神之眼」…",
			"voicelineType": "3001",
			"description": "「神之眼」，師父幫忙。雷的牙打架，保護「盧皮卡」。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9116,
			"title": "有什麼想要分享…",
			"voicelineType": "3002",
			"description": "晚上，星星很好看。天空，湖裡，都有很多很多星星。\n晚上，城市裡，也有很多星星。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_dialog_share"
		},
		{
			"voicelineId": 9117,
			"title": "感興趣的見聞…",
			"voicelineType": "3003",
			"description": "我的「盧皮卡」的狼。牠們是家人。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_pref_gift"
		},
		{
			"voicelineId": 9118,
			"title": "關於麗莎…",
			"voicelineType": "4001",
			"description": "師父的雷，天空會變黑。很可怕，很強。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_lisa_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				},
				{
					"unlockText": "完成「盧皮卡的意義」劇情",
					"conditionType": "FETTER_COND_FINISH_QUEST",
					"paramList": [
						45406
					]
				}
			]
		},
		{
			"voicelineId": 9119,
			"title": "關於芭芭拉…",
			"voicelineType": "4002",
			"description": "芭芭拉，亮晶晶的女孩子。很善良。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_barbara_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9120,
			"title": "關於迪盧克…",
			"voicelineType": "4003",
			"description": "迪盧克，紅紅的、黑黑的。又冷又熱。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_diluc_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9121,
			"title": "關於溫迪…",
			"voicelineType": "4004",
			"description": "詩歌…唔，不明白。雖然不明白，「盧皮卡」喜歡。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_venti_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9122,
			"title": "關於班尼特…",
			"voicelineType": "4005",
			"description": "班尼特，話很多，和他一起，吃不飽。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_bennett_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 9123,
			"title": "關於可莉…",
			"voicelineType": "4006",
			"description": "可莉，朋友！朋友，但是，頭疼。",
			"voicefile": "VO_friendship/VO_razor/vo_razor_teammate_klee_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
