{
	"id": 10000016,
	"name": "Diluc",
	"friendLines": [
		{
			"voicelineId": 8100,
			"title": "初次見面…",
			"voicelineType": "1000",
			"description": "蒙德城的迪盧克，應約而來。閒聊恕不奉陪，如果{F#妳}{M#你}是想做一番大事，我倒有點興致。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_draw_appear"
		},
		{
			"voicelineId": 8101,
			"title": "閒聊·放鬆",
			"voicelineType": "1101",
			"description": "偶爾悠閒一下，也不錯。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_explore_idle_01"
		},
		{
			"voicelineId": 8102,
			"title": "閒聊·騎士團",
			"voicelineType": "1102",
			"description": "應該換騎士團的那些家伙來陪{F#妳}{M#你}。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_explore_idle_02"
		},
		{
			"voicelineId": 8103,
			"title": "閒聊·行會",
			"voicelineType": "1103",
			"description": "行會裡還有很多事，{F#妳}{M#你}在這休息，我回去看看。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_explore_idle_03"
		},
		{
			"voicelineId": 8161,
			"title": "閒聊·準備",
			"voicelineType": "1106",
			"description": "提前做好準備，先於對方進攻吧。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_suit_aristocrat_idle_01"
		},
		{
			"voicelineId": 8162,
			"title": "閒聊·計畫",
			"voicelineType": "1107",
			"description": "休息時，也可以思考接下來的計畫。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_suit_aristocrat_idle_02"
		},
		{
			"voicelineId": 8163,
			"title": "閒聊·提醒",
			"voicelineType": "1108",
			"description": "任何行動難免都有風險，{F#妳}{M#你}要多加小心。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_suit_aristocrat_idle_03"
		},
		{
			"voicelineId": 8104,
			"title": "下雨的時候·傘",
			"voicelineType": "1201",
			"description": "我不用傘，{F#妳}{M#你}隨意。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_weather_heavyRain_01"
		},
		{
			"voicelineId": 8105,
			"title": "下雨的時候·清洗",
			"voicelineType": "1202",
			"description": "雨嗎？可惜洗刷不了那些汙穢的心。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_weather_heavyRain_02"
		},
		{
			"voicelineId": 8106,
			"title": "陽光很好…",
			"voicelineType": "1203",
			"description": "只要堅信自己的道路，就無所謂天氣是晴是雨。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_weather_sunny_01"
		},
		{
			"voicelineId": 8107,
			"title": "颳大風了…",
			"voicelineType": "1204",
			"description": "呼嘯的風聲，會被用來遮掩犯罪的痕跡。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_weather_gale_01"
		},
		{
			"voicelineId": 8108,
			"title": "早安…",
			"voicelineType": "1001",
			"description": "寒暄就免了。昨夜一切平安就好。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_greetingMorning"
		},
		{
			"voicelineId": 8109,
			"title": "午安…",
			"voicelineType": "1002",
			"description": "想試試隱藏菜單嗎？",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_greetingNoon"
		},
		{
			"voicelineId": 8110,
			"title": "晚上好…",
			"voicelineType": "1003",
			"description": "夜晚到了。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_greetingNight"
		},
		{
			"voicelineId": 8111,
			"title": "晚安…",
			"voicelineType": "1004",
			"description": "嗯，下次見。我會目送{F#妳}{M#你}安全離開的。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_greetingNight2"
		},
		{
			"voicelineId": 8112,
			"title": "關於迪盧克自己…",
			"voicelineType": "2001",
			"description": "「暗夜英雄」的傳說嗎？有趣的傳言。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_idle"
		},
		{
			"voicelineId": 8113,
			"title": "關於我們·需要",
			"voicelineType": "2002",
			"description": "需要什麼就直說。報帳問題找埃澤。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_close1"
		},
		{
			"voicelineId": 8114,
			"title": "關於我們·證明",
			"voicelineType": "2003",
			"description": "{F#妳}{M#你}已經證明了{F#妳}{M#你}的決心。那麼，也讓{F#妳}{M#你}見證一下我的意志吧。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_close2",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到6後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 8115,
			"title": "關於「神之眼」…",
			"voicelineType": "3001",
			"description": "對於無法看清眼前道路的人來說，「神之眼」或許是神的一盞明燈。但對於有信念的人來說，只是一個可有可無的獎章罷了。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_pendant",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8116,
			"title": "有什麼想要分享…",
			"voicelineType": "3002",
			"description": "我偶爾會去「天使的饋贈」親自調酒，因為酒館是情報集中流通的地方。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_share"
		},
		{
			"voicelineId": 8117,
			"title": "感興趣的見聞…",
			"voicelineType": "3003",
			"description": "…是夜梟。在黑暗中守望、觀察的猛禽，只在最合適的時機出手，沒有任何破綻。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_pref_gift"
		},
		{
			"voicelineId": 8118,
			"title": "關於凱亞…",
			"voicelineType": "4001",
			"description": "凱亞嗎…那個男人說的話，只能信一半。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_teammate_kaeya_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8119,
			"title": "關於麗莎…",
			"voicelineType": "4002",
			"description": "麗莎，能成為騎士團內最值得仰賴的人之一。如果她願意的話。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_teammate_lisa_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8120,
			"title": "關於琴·敵人",
			"voicelineType": "4003",
			"description": "無意義的瑣事，是琴最大的敵人。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_teammate_qin_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8121,
			"title": "關於琴·職責",
			"voicelineType": "4004",
			"description": "琴那無人能及的責任心，終究讓她沒能看清自己真正的職責啊。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_teammate_qin_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8122,
			"title": "關於莫娜…",
			"voicelineType": "4005",
			"description": "蒙德的土地浸潤著風神的恩澤，就不需要贊助占星術士來預測收成了吧。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_teammate_mona_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8123,
			"title": "關於迪奧娜…",
			"voicelineType": "4006",
			"description": "迪奧娜對我的態度很是生硬，我也一直沒找到合適的機會交流。如果是涉及到家族恩怨的話…嗯？不是？只是單純地討厭酒？沒想到在這蒙德，也會有討厭酒的酒保啊。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_teammate_diona_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8124,
			"title": "關於法爾伽…",
			"voicelineType": "4008",
			"description": "大團長法爾伽，我能理解他的立場，卻無法認同他的做派。呵…是我多言了。我早已不是西風騎士，忘了我說的話吧。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_teammate_varka_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8125,
			"title": "關於優菈…",
			"voicelineType": "4009",
			"description": "我聽說過她，她有自己的理念。努力掙脫家族束縛的氣勢也值得稱道。但她…卻加入了西風騎士團。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_teammate_eula_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8126,
			"title": "想要瞭解迪盧克·其一",
			"voicelineType": "5001",
			"description": "有何貴幹？",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_friendship_01"
		},
		{
			"voicelineId": 8127,
			"title": "想要瞭解迪盧克·其二",
			"voicelineType": "5002",
			"description": "嗯，{F#妳}{M#你}問騎士團？哼…沒什麼可多談的，道路不同罷了。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_friendship_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到3後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						3
					]
				}
			]
		},
		{
			"voicelineId": 8128,
			"title": "想要瞭解迪盧克·其三",
			"voicelineType": "5003",
			"description": "很久不和管家之外的人對弈了。難得有這份閒暇，跟我下局棋吧。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_friendship_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到4後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8129,
			"title": "想要瞭解迪盧克·其四",
			"voicelineType": "5004",
			"description": "將軍。哼，但能把我逼到這個地步，還真是不簡單啊。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_friendship_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到5後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						5
					]
				}
			]
		},
		{
			"voicelineId": 8130,
			"title": "想要瞭解迪盧克·其五",
			"voicelineType": "5005",
			"description": "{F#妳}{M#你}還不賴。無論是對弈還是戰鬥，都不乏讓人眼前一亮的表現。不…這樣的合作，對我來說也算新奇…那麼，今後也同行吧，「吾友」。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_friendship_05",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "好感等級達到6後開放",
					"conditionType": "FETTER_COND_FETTER_LEVEL",
					"paramList": [
						6
					]
				}
			]
		},
		{
			"voicelineId": 8131,
			"title": "迪盧克的愛好…",
			"voicelineType": "6001",
			"description": "莊園裡的那些藝術品？那不是我的。是先父有收集的愛好。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_pref_hobby"
		},
		{
			"voicelineId": 8132,
			"title": "迪盧克的煩惱…",
			"voicelineType": "6002",
			"description": "真正應當消滅的敵人，孕育罪惡的「黑暗」本身，需要用刀劍以外的方法去斬斷…",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_dialog_annoyed"
		},
		{
			"voicelineId": 8133,
			"title": "喜歡的食物…",
			"voicelineType": "6003",
			"description": "{F#妳}{M#你}問我杯子裡的是什麼酒？葡萄汁而已，我通常喝這個。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_pref_foodLike"
		},
		{
			"voicelineId": 8134,
			"title": "討厭的食物…",
			"voicelineType": "6004",
			"description": "我不喜歡酒。沒什麼…我不喜歡酒的口感。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_pref_foodDislike"
		},
		{
			"voicelineId": 8170,
			"title": "收到贈禮·其一",
			"voicelineType": "6007",
			"description": "很精緻。風味令人難忘。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_spice_pref_01"
		},
		{
			"voicelineId": 8171,
			"title": "收到贈禮·其二",
			"voicelineType": "6008",
			"description": "能品嚐出廚師的專注。不錯的菜餚。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_spice_normal_01"
		},
		{
			"voicelineId": 8172,
			"title": "收到贈禮·其三",
			"voicelineType": "6009",
			"description": "…謝謝。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_spice_dislike_01"
		},
		{
			"voicelineId": 8135,
			"title": "生日…",
			"voicelineType": "7001",
			"description": "生日快樂，這是屬於{F#妳}{M#你}的重要日子。說吧，{F#妳}{M#你}的心願。如果在我能力範圍之內，我會考慮的。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_pref_birthday",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"conditionType": "FETTER_COND_PLAYER_BIRTHDAY",
					"paramList": []
				}
			]
		},
		{
			"voicelineId": 8136,
			"title": "突破的感受·起",
			"voicelineType": "8001",
			"description": "為了達成目標，力量是必須的。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_starUp_01",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等級「1」階後開啟",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						1
					]
				}
			]
		},
		{
			"voicelineId": 8137,
			"title": "突破的感受·承",
			"voicelineType": "8002",
			"description": "前方會有什麼？我很少會思考這種問題。\n我必須前進，不論有誰阻擋在我面前。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_starUp_02",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等級「2」階後開啟",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						2
					]
				}
			]
		},
		{
			"voicelineId": 8138,
			"title": "突破的感受·轉",
			"voicelineType": "8003",
			"description": "力量增強了…雖然還不夠，但終歸是在向前方探索。\n在「黎明」到來之前，必須有人稍微照亮「黑暗」…",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_starUp_03",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等級「4」階後開啟",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						4
					]
				}
			]
		},
		{
			"voicelineId": 8139,
			"title": "突破的感受·合",
			"voicelineType": "8004",
			"description": "我原以為——要掃蕩罪惡的話，我必須獨自行走在長夜之中。\n但{F#妳}{M#你}的堅持讓我意識到，我又一次錯了。「同伴」，我向{F#妳}{M#你}致以感謝。",
			"voicefile": "VO_friendship/VO_diluc/vo_diluc_starUp_04",
			"hasUnlockConditions": true,
			"unlockConditions": [
				{
					"unlockText": "突破等級「6」階後開啟",
					"conditionType": "FETTER_COND_AVATAR_PROMOTE_LEVEL",
					"paramList": [
						6
					]
				}
			]
		}
